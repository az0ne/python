{% block custom_script %}
<script src="/static/js/mz-index.js"></script>
<script type="text/javascript" src="/static/js/layer/layer.min.js"></script>
<script type="text/javascript">

    /* 视频介绍 */    
    $(function(){
        $('#videoPopBtn').click(function(){
            $.layer({
                type : 1,
                title : false,
                closeBtn: [0, true],
                shade: [0.5, '#000'],
                area: ['800px', '440px'],
                bgcolor: '#000',
                border: [0],
                shift: 'top',
                page:{
                    html : videoCnt( 'http://www.w3school.com.cn/example/html5/mov_bbb.mp4' ),
                }
            });
        });
        function videoCnt(url){
            return '<video style="width:800px;height:440px" src="'+url+'" controls="controls"></video>'
        }
    });


    /* 院校专区介绍 */
    /* tab-btn-list 切换 */
    $(function(){
        var btn = $('.tab-btn-list .itm'), cnt = $('.tab-cnt-list .itm'), arrow = $('.colleges-tab-cnt-box .arrow');
        btn.each(function(i,o){
            o.index = i;
        }).hover(function(){
            run(this);
        });
        run( btn.eq(0)[0] );
        function run(that){
            if(btn.cur == that)return;
            clearTimeout(btn.t);
            btn.t = setTimeout(function(){
                var o, s='itm-act';
                o = btn.removeClass(s).eq(that.index).addClass(s);
                cnt.hide().eq(that.index).stop().fadeIn();
                arrow.stop().animate({ 'left' : o.offset().left + o.width()/2 - 15 });
                btn.cur = that;
            },50);
        }
    });


    /* 热门省份及城市 */
    /* hot-tag-box 切换院校 */
    $(function(){
        var tags = $('#hotTagList a'), collegeBox = $('#hotTagColleges');
        collegeBox.parent().css({'minHeight' : 300});
        tags.click(function(){
            var that = $(this);
            tags.removeClass('act');
            that.addClass('act');
            var city_id = that.attr('id').split("_")[1];
            $.ajax({
                type: "POST",
                url: "/academic/school/list/",
                data:{'city_id':city_id},
                dataType: "json",
                success: function(data){
                    editHtml(data);
                }
            });

        });
        function editHtml(json){
            var html='';
            $.each(json,function(k,o){
                html += '<li class="col-xm-3 col-md-3 col-lg-3 marg-bottom">';
                html += '<div class="itm"><a href="';
                html += o.href || 'javascript:;';
                html += '"><span class="img"><img src="';
                html += o.src || 'about:blank';
                html += '" alt="';
                html += o.name;
                html +='"/></span>';
                html += '</a></div></li>';
            });
            collegeBox.hide().html( html ).stop().fadeIn();
        }

        // 默认显示第一个tag
        tags.eq(0).click();
    });
  </script>
{% endblock %}

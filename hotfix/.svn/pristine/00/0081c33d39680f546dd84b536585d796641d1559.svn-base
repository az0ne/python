@import 'core/reset';
@import 'core/common';
@import 'core/var';
@import 'core/function';
@import 'core/mixin';
@import 'studyPanelCommon';

$bg2 : url(../images/lps4/sprite2.png) 0 0 no-repeat;
$bg3 : url(../images/lps4/sprite3.png) 0 0 no-repeat;
$onevonebg:url(/images/teaOneVOneIcon.png) no-repeat;
$gou: url('data:image/png;base64,') 0 0 no-repeat;
$arrow_right: url('data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAA4AAAAMCAYAAABSgIzaAAAAAXNSR0IArs4c6QAAAFxJREFUKBVjYMAD/v//vxWIjwCxKB5lmFJQTUDq/xWSNIMUQzUBqQHXDHTCFpA7SARHmDCDhIoiQNeQHkj00wTyKdA2UKoBAYwEQChwPgL1HwViR0ZGxtcgw2AAAMjOzH1WRGEMAAAAAElFTkSuQmCC') 0 0 no-repeat;
$step_active: url('data:image/png;base64,') 0 0 no-repeat;
$step_nonactive: url('data:image/png;base64,') 0 0 no-repeat;
$qq: url('data:image/png;base64,') 0 0 no-repeat;
$wx: url('data:image/png;base64,') 0 0 no-repeat;
$phone: url('data:image/png;base64,') 0 0 no-repeat;

%images{
    float: left;
    position: relative;
    p{
        width: 56px;
        height: 56px;
        border-radius: 50%;
        overflow: hidden;
        float: left;
        img{
            width: 100%;
            border-radius: 50%;
        }
    }
}
%times{
    span{
        display: block;
        font-size: 18px;
        color: #b4b4b4;
    }
    strong{
        font-weight: normal;
        @extend span;
        font-size: 24px;
        color: #6f6f6f;
    }
}
.study-panel {
    .main-center {
        min-width: 1080px !important;
        &:before, &:after {
            @extend .CLEARFIX;
        }
    }
    .preloader {
        position: fixed;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        z-index: 99999;
        background-color: #FFF;
        .load {
            position: absolute;
            left: 50%;
            top: 50%;
            margin-left: -125px;
            margin-top: -125px;
        }
        p {
            @extend .textC;
            @extend .font16;
        }
    }
    .last-time {
        height: 78px;
        background-color: #fff9ea;
        position: relative;
        margin-right: 20px;
        .pos-l, .pos-r {
            position: absolute;
        }
        .pos-l {
            left: 16px;
            top: 25px;
            @extend .font14;
            color: #666;
            span {
                color: #ff8a00;
                &:before, &:after {
                    content: '';
                    @extend .vam;
                    margin-top: -2px;
                }
                &:before {
                    width: 18px;
                    height: 30px;
                    background: $bg;
                    margin-right: 8px;
                    background-position: 0 -561px;
                }
                &:after {
                    width: 6px;
                    height: 10px;
                    margin: 0 9px;
                    background: $bg;
                    background-position: 0 -601px;
                }
            }
        }
        .pos-r {
            right: 16px;
            top: 21px;
            width: 147px;
            height: 37px;
            line-height: 37px;
            color: #fff;
            @extend .font14;
            @extend .textC;
            background-color: #ff6e02;
            background: -webkit-linear-gradient(#ffa901, #ff6e02);
            background: -o-linear-gradient(#ffa901, #ff6e02);
            background: -moz-linear-gradient(#ffa901, #ff6e02);
            background: linear-gradient(#ffa901, #ff6e02);
        }
    }
    .left-main, .right-side {
        margin-top: 25px;
    }
    .left-main {
        height: 100%;        
        margin-right: 323px;
        .ad {
            display: block;
            margin-bottom: 20px;
            background: url(../images/lps4/ad_0.png) 0 0 no-repeat;
            background-size: cover;
            height: 188px;
            .left, .right {
                display: table-cell;
                vertical-align: top;
            }
            .left {
                float: left;
                width: 234px;
                padding: 24px 0 24px 24px;
            }
            .right {
                display: block;
                margin-left: 234px;
                padding: 24px 17px 24px 24px;
                height: 100%;
                position: relative;
                h3 {
                    @extend .font20;
                    color: #02c99e;
                    margin-bottom: 18px;
                    &:before {
                        content: '';
                        @extend .vam;
                        width: 15px;
                        height: 19px;
                        background: $bg2;
                        margin-right: 10px;
                        margin-top: -2px;
                        background-position: 0 -447px;
                    }
                }
                p {
                    @extend .font14;
                    color: #7892a3;
                    margin-bottom: 20px;
                }
                .detail {
                    @extend .font14;
                    color: #fff;
                }

                div {
                    position: absolute;
                    right: 12px;
                    bottom: 20px;
                    span {
                        display: block;
                        @extend .font14;
                        @extend .textR;
                        margin-right: 130px;
                    }
                    .yuan-sale {
                        color: #7ea0b7;
                        text-decoration: line-through;
                    }
                    .sale {
                        color: #fff;
                    }
                }
                .go-buy {
                    position: absolute;
                    right: 12px;
                    bottom: 20px;
                    width: 110px;
                    line-height: 36px;
                    color: #fff;
                    @extend .font16;
                    @extend .textC;
                    background: #ff6e02;
                    background: -webkit-linear-gradient(#ffa901, #ff6e02);
                    background: -o-linear-gradient(#ffa901, #ff6e02);
                    background: -moz-linear-gradient(#ffa901, #ff6e02);
                    background: linear-gradient(#ffa901, #ff6e02);
                }
            }
        }
    }
    .main-content {
        background-color: #fff;
        padding-bottom: 37px;
        box-shadow: 0 3px 8px rgba(0,0,0,0.09);
        position: relative;
        &:after {
            content: '';
            display: block;
            height: 37px;
            width: 100%;
            background-color: #fff;
            position: absolute;
            left: 0;
            bottom: 0;
        }
    }
    .right-side {
        width: 283px;
        @extend .fr;
        margin-right: 20px;
    }
    .study-stage {
        font-size: 0;
    }
    .study-stage-progress {
        width: 4px;
        background-color: #ebebeb;
        @extend .vam;
        margin-right: 20px;
        margin-left: 20px;
        position: relative;
        margin-top: 26px;
        margin-bottom: -26px;        
        .progress-on {
            height: 20%;
            background-color: #00d592;
        }
        &:before,&.dnf:before {
            content: '';
            @extend .vam;
            position: absolute;
            top: -4px;
            left: -6px;
            width: 16px;
            height: 16px;
            background: $bg;
            background-position: -44px -1154px;
        }
        &.complete:before {
            background-position: 0 -1154px;
        }
        &.dnf:before {
            background-position: -44px -1154px;
        }
    }
    .study-stage-box {
        @extend .vam;
        h2 {
            @extend .font14;
            @extend .bold;
            line-height: 60px;
            color: #636363;
        }
    }
    .study-stage-ball {        
        height: auto;
        min-height: 175px;
        min-width: 175px;
        background-color: #f0f2f4;
        border-right: 1px solid #ebebeb;
        border-bottom: 1px solid #ebebeb;
        overflow: hidden;
        position: relative;        
        .lists {
            li {
                width: 174px;
                height: 175px;
                position: absolute;
                color: #000;
                @extend .textC;
                cursor: pointer;
                left: 0;
                top: 0;
                right: 0;
                border-left: 1px solid #ebebeb;
                border-top: 1px solid #ebebeb;
                background-color: #fff;
                border-right: none;
                & > div {
                    position: relative;
                    width: 100%;
                    height: 100%;
                    padding-top: 27px;
                    &:before,&:after {
                        content: '';
                        width: 0;
                        height: 0;
                        position: absolute;

                        z-index: 1;
                    }
                }
                span {
                    display: block;
                    width: 78px;
                    height: 94px;
                    margin: 0 auto 14px;
                }
                p {
                    @extend .font12;
                    @extend .vam;
                    color: #707071;
                    border-radius: 50px;
                    padding: 0 10px;
                    background-color: #ebebeb;
                    line-height: 24px;
                    max-width: 146px;
                    @extend .substr;
                }
            }
            .add-arrow-r {
                &:before,&:after {
                    top: 50%;
                    margin-top: -10px;
                    border-top: 10px solid transparent;
                    border-bottom: 10px solid transparent;
                }
                &:before {
                    border-left: 10px solid #ebebeb;
                    right: -10px;
                }
                &:after {
                    border-left: 10px solid #fff;
                    right: -8px;
                }
            }
            .add-arrow-l {
                &:before,&:after {
                    top: 50%;
                    margin-top: -10px;
                    border-top: 10px solid transparent;
                    border-bottom: 10px solid transparent;
                }
                &:before {
                    border-right: 10px solid #ebebeb;
                    left: -10px;
                }
                &:after {
                    border-right: 10px solid #fff;
                    left: -8px;
                }
            }
            .add-arrow-b {
                &:before,&:after {
                    left: 50%;
                    margin-left: -10px;
                    border-right: 10px solid transparent;
                    border-left: 10px solid transparent;
                    border-bottom: 10px solid transparent;
                }
                &:before {
                    border-top: 10px solid #ebebeb;
                    bottom: -20px;
                }
                &:after {
                    border-top: 10px solid #fff;
                    bottom: -18px;
                }
            }
        }

        .finish {
            span {
                background: url(../images/lps4/ball_1.png) 0 0 no-repeat;
            }
        }
        .lock {
            span {
                background: url(../images/lps4/ball_5.png) 0 0 no-repeat;
            }
        }
        .unlock {
            span {
                background: url(../images/lps4/ball_3.png) 0 0 no-repeat;
            }
        }
        .ongoing {
            span {
                background: url(../images/lps4/ball_2.png) 0 0 no-repeat;
            }
        }
        .cutdwon-oneday {
            span {
                position: relative;
            }
            i {
                display: block;
                width: 41px;
                height: 41px;
                position: absolute;
                right: -10px;
                bottom: 8px;
                background: $bg;
                background-position: 0 -1180px;
            }
        }
        .score {
            span {
                background: url(../images/lps4/ball_4.png) 0 0 no-repeat;
            }
        }
        .current {
            span {
                background: url(../images/lps4/ball_12.gif) 0 0 no-repeat;
            }
        }
        .rebuild {
            span {
                background: url(../images/lps4/ball_7.png) 0 0 no-repeat;
            }
        }
        .rebuilding {
            span {
                position: relative;
                background: url(../images/lps4/ball_6.png) 0 0 no-repeat;
            }
            i {
                display: block;
                width: 41px;
                height: 41px;
                position: absolute;
                right: -10px;
                bottom: 8px;
                background: $bg;
                background-position: 0 -1292px;
            }
        }

        .hover-tips {
            display: none;
            position: absolute;
            left: 174px;
            top: 0;
            width: 174px;
            height: 175px;
            border: 1px solid transparent;
            z-index: 12;
            &:before {
                content: '';
                width: 0;
                height: 0;
                float: right;
                margin-right: -10px;
                margin-top: 77px;
                border-top: 10px solid transparent;
                border-bottom: 10px solid transparent;
            }
            h4 {
                padding: 15px 15px 15px 10px;
                @extend .colorff;
                @extend .font14;
                word-wrap: break-word;
            }
            p {
                position: absolute;
                bottom: 0;
                left: 0;
                @extend .font12;
                width: 100%;                
                span {
                    display: block;
                    padding: 0 10px;
                    
                }
                .first {
                    margin-bottom: 10px;
                    &:before {
                        content: '';
                        @extend .vam;
                        height: 13px;
                        margin-top: -2px;
                        margin-right: 5px;
                    }
                }
                .two {
                    line-height: 21px;
                }
            }
        }       
        .left-arrow:before {
            float: left;
            margin-left: -10px;
            border-left: none;
        }
        .orange {
            background-color: #ff9c00;
            &:before {
                border-left: 10px solid #ff9c00;
            }
            &.left-arrow:before {
                border-left: none;
                border-right: 10px solid #ff9c00;
            }
            p {
                color: #ffdcaf;
                .first {
                    &:before {
                        border-left: 4px solid #ffdcaf;
                    }
                }
            }
        }
        .green {
            background-color: #15d5ab;
            &:before {
                border-left: 10px solid #15d5ab;
            }
            &.left-arrow:before {
                border-left: none;
                border-right: 10px solid #15d5ab;
            }
            p {
                color: #288e78;
                .first {
                    &:before {
                        border-left: 4px solid #288e78;
                    }
                }
                .two {
                    background-color: #27947c;
                    color: #3bc9aa;
                }
            }
        }
        .red {
            background-color: #f83d3d;
            &:before {
                border-left: 10px solid #f83d3d;
            }
            &.left-arrow:before {
                border-left: none;
                border-right: 10px solid #f83d3d;
            }
            p {
                color: #ffc6c6;                
                .first {
                    &:before {
                        border-left: 4px solid #ffc6c6;
                    }
                }
            }
        }
    }
    .live-appointment {
        &>div{padding: 0 20px 27px;background-color: #fff;}
        box-shadow: 0 3px 8px rgba(0,0,0,0.09);
        margin-bottom: 22px;
        h2 {
            @extend .font14;
            color: #636363;
            line-height: 52px;
            border-bottom: 1px solid #e5e5e5;
            padding: 0 12px 0 14px;
            position: relative;
            background: #fff;
            &:before {
                content: '';
                @extend .vam;
                border-left: 4px solid #15d5ab;
                height: 15px;
                margin-right: 8px;
                margin-top: -2px;
            }
            .live-prev {
                position: absolute;
                top: 14px;
                right: 12px;
                width: 60px;
                line-height: 24px;
                border: 1px solid #15d5ab;
                color: #15d5ab;
                border-radius: 50px;
                @extend .textC;
                &:after {
                    content: '';
                    z-index: -1;
                    @extend .vam;
                    width: 6px;
                    height: 8px;
                    background: $bg;
                    background-position: 0 -621px;
                    margin-top: -2px;
                    margin-left: 5px;
                }
            }
        }

        .date-time {
            @extend .vam;
            width: 100%;
            span {
                @extend .font18;
                color: #737373;
            }
        }

        .live-time {
            @extend .bold;
            @extend .font24;
            @extend .textC;
            color: #636363;
            background-color: #253b4b;
            margin-bottom: 10px;
        }

        .appointment-btn {
            display: block;
            height: 45px;
            line-height: 45px;
            @extend .textC;
            @extend .font14;
            @extend .colorff; 
            background-color: #00e767;
            background: -webkit-linear-gradient(#00e767, #00d4ab);
            background: -o-linear-gradient(#00e767, #00d4ab);
            background: -moz-linear-gradient(#00e767, #00d4ab);
            background: linear-gradient(#00e767, #00d4ab);
        }
        .enter {
            background: #1e313e;
            padding: 15px 14px 27px;
            .tip{color: #15d5ab;font-size: 16px;}
            .times{
                background: #182a36;
                margin:14px 0 15px;
                padding: 10px 10px 16px;
                .live-time{color: #95b5ca;}
            }
            .txt {
                color: #516a7b;
                font-size: 14px;
                line-height: 24px;
                margin: 12px 0 18px;
                position: relative;
                height: 72px;
                overflow: hidden;
                a {
                    color: #95b5ca;
                    text-decoration: underline;
                    position: absolute;
                    right: 0;
                    bottom: 0;
                    padding-left: 30px;
                    background-color: #1e313e;
                    background: -webkit-gradient(linear, right top, left top, from(#1e313e), color-stop(0.75, #1e313e), to(rgba(30, 49, 62, 0)));
                    background: -webkit-linear-gradient(right, #1e313e 0%, #1e313e 75%, rgba(30, 49, 62, 0) 100%);
                    background: -o-linear-gradient(right, #1e313e 0%, #1e313e 75%, rgba(30, 49, 62, 0) 100%);
                    background: -moz-linear-gradient(right, #1e313e 0%, #1e313e 75%, rgba(30, 49, 62, 0) 100%);
                    background: -ms-linear-gradient(right, #1e313e 0%, #1e313e 75%, rgba(30, 49, 62, 0) 100%);
                }
            }
        }
    }
    .old-live {
        box-shadow: 0 3px 8px rgba(0,0,0,0.09);
        margin-bottom: 22px;
        background-color: #fff;
        h2 {
            @extend .font14;
            color: #636363;
            line-height: 52px;
            padding: 0 12px 0 14px;
            position: relative;
            &:before {
                content: '';
                @extend .vam;
                border-left: 4px solid #15d5ab;
                height: 15px;
                margin-right: 8px;
                margin-top: -2px;
            }
            .live-prev {
                position: absolute;
                top: 14px;
                right: 12px;
                width: 60px;
                line-height: 24px;
                border: 1px solid #15d5ab;
                color: #15d5ab;
                border-radius: 50px;
                @extend .textC;
                &:after {
                    content: '';
                    @extend .vam;
                    width: 6px;
                    height: 8px;
                    background: $bg;
                    background-position: 0 -621px;
                    margin-top: -2px;
                    margin-left: 5px;
                }
            }
        }
    }
    .skill-atlas {
        background-color: #15d5ab;
        box-shadow: 0 3px 8px rgba(0,0,0,0.09);
        margin-bottom: 22px;
        h2 {
            @extend .font14;
            @extend .colorff;
            line-height: 52px;
            border-bottom: 1px solid rgba(0,0,0,0.05);
            padding: 0 12px 0 14px;
            &:before {
                content: '';
                @extend .vam;
                border-left: 4px solid #0fb98d;
                height: 15px;
                margin-right: 8px;
                margin-top: -2px;
            }
        }
    }
    .skill-atlas-map {
        height: 294px;
        border-top: 1px solid rgba(235,235,235,0.4);
        background: #15d5ab url(../images/lps4/skill.png) 0 0 no-repeat;
        position: relative;
        a {
            @extend .font12;
            @extend .colorff;
            position: absolute;
        }
        .link-1 {
            top: 24px;
            left: 162px;
        }
        .link-2 {
            top: 54px;
            right: 16px;
        }
        .link-3 {
            bottom: 68px;
            right: 16px;
        }
        .link-4 {
            bottom: 36px;
            @extend .textC;
            left: 0;
            right: 0;
            margin: 0 auto;
        }
        .link-5 {
            bottom: 68px;
            left: 25px;
        }
        .link-6 {
            top: 54px;
            left: 25px;
        }
    }
    .ability-tree {
        padding: 0 28px 27px;
        box-shadow: 0 3px 8px rgba(0,0,0,0.09);
        margin-bottom: 22px;
        background-color: #fff;
        h2 {
            @extend .font14;
            color: #636363;
            line-height: 52px;
            border-bottom: 1px solid #e5e5e5;
            padding: 0 12px 0 14px;
            position: relative;
            margin: 0 -28px;
            &:before {
                content: '';
                @extend .vam;
                border-left: 4px solid #15d5ab;
                height: 15px;
                margin-right: 8px;
                margin-top: -2px;
            }
        }
        .ability-tree-lists {
            padding: 40px 0 0;
            .first, .finish {
                width: 100%;                
            }
            .first {
                height: 71px;
                background: url(../images/lps4/trunk_3.png) center no-repeat;
            }
            .first.finish {
                background: url(../images/lps4/trunk_2.png) center no-repeat;
            }
            dd {
                line-height: 28px;
                @extend .textC;
                @extend .font12;
                color: #9c9b9b;
                background-color: #ececec;
                border-radius: 50px;
                &:before {
                    content: '';
                    @extend .vam;
                    width: 18px;
                    height: 18px;
                    margin-right: 8px;
                    margin-top: -2px;
                    background: $bg;
                    background-position: 0 -667px;
                }
                &.finish {
                    &:before {
                        background-position: 0 -639px;
                    }
                }
                &.on {
                    border-radius: 0;
                    background-color: #fff;
                    &:before {
                        width: 0;
                        height: 0;
                        margin: 0;
                        @extend .fl;
                    }
                    .ability-dot {
                        line-height: 36px;
                        background-color: #ececec;
                        margin-bottom: 1px;
                        color: #9c9b9b;
                        @extend .substr;
                        padding: 0 8px;
                        &:before, &.now:before {
                            content: '';
                            width: 10px;
                            height: 10px;
                            @extend .vam;
                            border-radius: 50%;
                            margin-right: 4px;
                            margin-top: -2px;
                            margin-left: -2px;
                        }
                        &:before {
                            background-color: #dcdbdb;
                        }
                        &.now {
                            color: #fff;
                            background-color: #38e0a9;
                            &:before {
                                background-color: #fff;
                            }
                        }
                    }
                    .start {
                        border-radius: 10px 10px 0 0;
                    }
                    .end {
                        border-radius: 0 0 10px 10px;
                        margin-bottom: 0;
                    }
                }
            }
            dt, dt.finish {
                width: 6px;
                height: 34px;
                margin: 0 auto;
            }
            dt {                
                background-color: #ececec;
                &.finish {
                    background-color: #38e0a9;
                }
            }
            dt.last, dt.last.finish {
                width: 100%;
            }
            .finish {
                background-color: #38e0a9;
                color: #fff;
            }

            dt.last {
                height: 132px;
                background: url(../images/lps4/trunk_0.png) center no-repeat;
            }
            dt.last.finish {
                background: url(../images/lps4/trunk_1.png) center no-repeat;
            }
        }
    }
    .join-class-agreement {
        background-color: #fff;

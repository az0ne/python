<script type="text/javascript">
	$(function(){

		// 左边栏内容过多时，滚动
		(function(){
			var tabCnt = $('#sidebar_body .tab-cnt');
			var tabCntIn = tabCnt.find('.tab-cnt-in');
			tabCntIn.width( tabCntIn.width() );
			tabCnt.jScrollPane({
				verticalDragMinHeight: 50,
				horizontalDragMinWidth: 50
			});
			var scrollBar = tabCnt.find('.jspVerticalBar');
			scrollBar.hide();
			tabCnt.hover(function(){
				scrollBar.fadeIn();
			},function(){
				scrollBar.fadeOut();
			});
		}());

		// 总览--完成度
		(function(){
			// json数据
			var chartData = [{
					'value': 25,
					'color': "#FFE68C",
					'highlight': "#feedb1",
					'label': "未完成"
				},
				{
					'value': 75,
					'color':"#5ecfba",
					'highlight': "#70e0cb",
					'label': "已完成"
				}];
			// 图表实例
			$.chartExtend.Pie({
				'canvas' : '#canvas_1',
				'data' : chartData
			});
		}());

		// 完成计划
		(function(){
			// json数据
			var chartData = [{
					'date': '04/13',
					'plan_study_time': 100,
					'real_study_time': 80
				},{
					'date': '04/06',
					'plan_study_time': 100,
					'real_study_time': 120
				}];
			// 图表实例
			$.chartExtend.Line({
				'canvas' : '#canvas_2',
				'data' : chartData,
				'legend' : [{
						'dataKey' : 'real_study_time',
						'label' : '实际完成',
						'lineColor' : '#FFE68C'
					},{
						'dataKey' : 'plan_study_time',
						'label' : '计划',
						'lineColor' : '#5ecfba'
					}],
				'suf' : '%' //后缀
			});

		}());

		// 评测分数
		(function(){
			// json数据
			var chartData = [{
					//'stage':1,
					//'course':2,
					'course_name': 'IOS基础',
					'course_score':80
				},{
					//'stage':1,
					//'course':3,
					'course_name': 'Android基础',
					'course_score':90
				}];
			// 图表实例
			$.chartExtend.Bar({
				'canvas' : '#canvas_3',
				'data' : chartData
			});

		}());

		// 素质雷达图
		(function(){
			// json数据
			var chartData = [{
				'chart_capacity_alto': 60, // 汇总的沟通能力数值
				'exe_capacity_alto': 80, // 汇总的执行能力数值
				'initiative_alto': 88 //汇总的主动性数值
			}];
			// 图表实例
			$.chartExtend.Radar({
				'canvas' : '#canvas_5',
				'data' : chartData,
				// 字段描述
				'labels' : [{
					'dataKey' : 'chart_capacity_alto',
					'label' : '沟通能力'
				},{
					'dataKey' : 'exe_capacity_alto',
					'label' : '执行能力'
				},{
					'dataKey' : 'initiative_alto',
					'label' : '主动性'
				}],
				// 图例说明
				'legend' : [{
					'label' : '综合能力',
					'lineColor' : 'rgba(94, 207, 186,1)',
					'fillColor' : 'rgba(94, 207, 186,0.5)'
				}]
			});
		}());

		// 素质变化曲线
		(function(){
			// json数据
			var chartData = [{
					'chart_capacity': 60,
					'exe_capacity': 80,
					'initiative': 88,
					'date':'04/06'
				},{
					'chart_capacity': 60,
					'exe_capacity': 80,
					'initiative': 88,
					'date':'04/13'
				}];
			// 图表实例
			$.chartExtend.Line({
				'canvas' : '#canvas_6',
				'data' : chartData,
				'legend' : [{
						'dataKey' : 'chart_capacity',
						'label' : '执行力',
						'lineColor' : '#FFE68C'
					},{
						'dataKey' : 'exe_capacity',
						'label' : '主动性',
						'lineColor' : '#5ecfba'
					},{
						'dataKey' : 'initiative',
						'label' : '沟通力',
						'lineColor' : '#cfba5e'
					}],
				'suf' : '%' //后缀
			});

		}());




		// 左边栏tab
		$.commonTool.tab({
			'tabBox' : '#tab_box_1',
			'transitionCallback' : function(){}
		});

		// 学习数据tab
		$.commonTool.tab({
			'tabBox' : '#tab_box_2',
			'transitionCallback' : function(){}
		});

		// 个人素质tab
		$.commonTool.tab({
			'tabBox' : '#tab_box_3',
			'transitionCallback' : function(){}
		});


		// 加锁
		$.commonTool.addLock('.p-main .main .box-body');

	});

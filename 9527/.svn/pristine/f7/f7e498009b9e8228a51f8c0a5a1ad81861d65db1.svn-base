<!--搜索框-->
<div class="input-group mySearch">
    <input type="text" id="txt_search" class="form-control" placeholder="输入查询关键字"/>
	<span class="input-group-btn">
						<button type="button" class="btn btn-default" id="btn_search">
                            <span class="glyphicon glyphicon-search"></span>
	</button>
	</span>
</div>
<!-- 扩展功能栏-->
<div class="function_btn">
    <ul class="nav navbar-btn">
        <li>
            <button type="button" id="btn_addUserCenterAd" class="btn btn-default btn-info btn-sm active">
                <span class="glyphicon glyphicon-plus">添加个人中心广告</span>
            </button>
        </li>
    </ul>
</div>
<!--修改个人中心广告模态框-->
<div class="modal fade" id="modal_editUserCenterAd" role="dialog">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header btn-success active">
                <button type="button" class="close" data-dismiss="modal" aria-lable="close"><span
                        aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" style="text-shadow:3px 3px 2px #000">
                    修改个人中心广告信息</h4>
            </div>
            <form enctype="multipart/form-data" id="form_editUserCenterAd">
                <div class="modal-body" style="height: 160px;">
                    <div style="float: left ">
                        <img class="previewImg" src=""/>
                    </div>
                    <div style="float: left">
                        <div class="form-group">
                            <label class="inputAreaTitle">广告图片:</label>
                            <input class="inputArea file_imgUrl" type="file" accept="image/*" name="img_url"
                                   id="file_edit_imgUrl" placeholder="选择上传图片"/>
                            <span class="inputAreaError lbl_imgUrlError"></span>
                        </div>
                        <div class="form-group">
                            <label class="inputAreaTitle">图片标题:</label>
                            <input class="inputArea txt_imgTitle" type="text" name="img_title" id="txt_edit_imgTitle"
                                   placeholder="请输入图片标题"/>
                            <span class="inputAreaError lbl_imgTitleError"></span>
                        </div>
                        <div class="form-group">
                            <label class="inputAreaTitle">当前状态:</label>
                            <select class="inputArea slt_isActived" id="slt_edit_isActived" name="is_actived">
                                <option value="0">不激活</option>
                                <option value="1">激活</option>
                            </select>
                            <span class="inputAreaError lbl_isActivedError"></span>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <span id="isEditSuccess" style="float: left;display: none"></span>
                    <button type="button" class="btn btn-danger active" data-dismiss="modal" id="btn_closeModal">关闭
                    </button>
                    <button type="button" class="btn btn-primary active btn_careerCategory"
                            id="btn_saveEditUserCenterAd">保存
                    </button>
                </div>
            </form>
        </div>

    </div>
</div>
<!--添加个人中心广告模态框-->
<div class="modal fade" id="modal_addUserCenterAd" role="dialog">
    <div class="modal-dialog" role="document" style="background-color:#ffffff">
        <div class="modal-header btn-success active">
            <button type="button" class="close" data-dismiss="modal" aria-lable="close"><span
                    aria-hidden="true">&times;</span></button>
            <h4 class="modal-title" style="text-shadow:3px 3px 2px #000">
                添加个人中心广告信息</h4>
        </div>
        <form enctype="multipart/form-data" id="form_addUserCenterAd">
            <div class="modal-body" style="height: 150px;">
                <div style="float: left ">
                    <img class="previewImg" src=""/>
                </div>
                <div style="float: left">
                    <div class="form-group">
                        <label class="inputAreaTitle">广告图片:</label>
                        <input class="inputArea file_imgUrl" name="img_url" accept="image/*" type="file" id="file_add_imgUrl"
                               placeholder="选择上传图片"/>
                        <span class="inputAreaError lbl_imgUrlError">图片格式:bmp,png,gif,jpeg,jpg</span>
                    </div>
                    <div class="form-group">
                        <label class="inputAreaTitle">图片标题:</label>
                        <input class="inputArea txt_imgTitle" name="img_title" type="text" id="txt_add_imgTitle"
                               placeholder="请输入图片标题"/>
                        <span class="inputAreaError lbl_imgTitleError"></span>
                    </div>
                    <div class="form-group">
                        <label class="inputAreaTitle">当前状态:</label>
                        <select class="inputArea slt_isActived" id="slt_add_isActived" name="is_actived">
                            <option value="0">不激活</option>
                            <option value="1">激活</option>
                        </select>
                        <span class="inputAreaError lbl_isActivedError"></span>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <span id="isAddSuccess" style="float: left;display: none"></span>
                <button type="button" class="btn btn-danger active" data-dismiss="modal" id="btn_closeModal">关闭
                </button>
                <button type="button" class="btn btn-primary active btn_careerCategory"
                        id="btn_saveAddUserCenterAd">保存
                </button>
            </div>
        </form>
    </div>
</div>
</div>

<!--查看个人中心广告模态框-->
<div class="modal fade" id="modal_showUserCenterAd" role="dialog">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header btn-success active">
                <button type="button" class="close" data-dismiss="modal" aria-lable="close"><span
                        aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" style="text-shadow:3px 3px 2px #000" id="title_showUserCenterAdLab">
                    查看个人中心广告信息</h4>
            </div>
            <div class="modal-body" style="height: 150px;">
                <div class="form-group" style="float: left;height: 300px;width: 150px">
                    <p style="">广告图片:</p>
                    <img id="lbl_imgUrl" src="" height="90px" width="90px"/>
                </div>
                <div style="float: right;width: 300px;height: 150px">
                    <div class="form-group">
                        <p>图片标题:</p>
                        <label id="lbl_imgTitle"></label>
                    </div>
                    <div class="form-group">
                        <p>当前状态:</p>
                        <label id="lbl_isActived"></label>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!--刪除个人中心广告警示框-->
<div class="modal fade" id="modal_delUserCenterAd" role="dialog">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header btn-danger active">
                <button type="button" class="close" data-dismiss='modal' aria-lable='close'><span
                        aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" style="text-shadow:3px 3px 2px #000" id="title_delUserCenterAdLab">
                    删除个人中心广告信息</h4>
            </div>
            <div class="modal-body">
                <h3 class="text-danger text-center active" id="delUserCenterAdMessage"></h3>
            </div>
            <div class="modal-footer">
                <span class="text-danger selectRowId" style="float: left;display: none"></span>
                <button type="button" class="btn btn-danger active" data-dismiss="modal">关闭</button>
                <button type="button" class="btn btn-primary active" id="btn_ensureDelUserCenterAd">确定</button>
            </div>
        </div>
    </div>
</div>
<!--显示原始图片-->

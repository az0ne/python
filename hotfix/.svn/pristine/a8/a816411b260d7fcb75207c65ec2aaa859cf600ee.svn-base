    padding-top: 1.25rem
}

.personal-page-top p {
    width: 2.8rem;
    height: 2.8rem;
    display: inline-block;
    border-radius: 100%;
    border: .1rem solid #fff;
    overflow: hidden;
    box-shadow: 0 0 0 0.25rem rgba(0, 84, 74, 0.1)
}

.personal-page-top strong {
    font-size: .75rem;
    color: #fff;
    margin-top: .2rem
}

.personal-page-abstract p {
    font-size: .6rem;
    color: #666;
    padding: .5rem 4%;
    line-height: .9rem;
    height: 1px;
    display: table
}

#wiki_lists li {
    font-size: 0;
    border: none
}


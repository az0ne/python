.personalCmainLmenu {
	display: table;
	width: 100%;
	background: #fff;
	border-radius: 2px;
	overflow: hidden;
	margin-bottom: 20px;
	li {
		float: left;
		width: 100%;
		height: 52px;
		border-bottom: 1px solid #f5f5f5;
		&:last-child {
			border: 0;
		}
		& > a {
			display: block;
			width: 100%;
			height: 52px;
			line-height: 52px;
			color: #666;
			.i1 {
				margin-right: 20px;
				margin-left: 22px;
			}
			.i2 {
				display: none;
			}
			span {
				display: inline-block;
				vertical-align: middle;
				width: 106px;
				white-space: nowrap;
				overflow: hidden;
				text-overflow: ellipsis;
				margin-right: 10px;
				line-height: 1;
			}
		}
	}
}



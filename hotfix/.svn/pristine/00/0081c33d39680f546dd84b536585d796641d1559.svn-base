            background: -o-linear-gradient(#ffa901, #ff6e02);
            background: -moz-linear-gradient(#ffa901, #ff6e02);
            background: linear-gradient(#ffa901, #ff6e02);
            i{
                display: inline-block;
                height: 12px;
                width: 14px;
                margin-left: 5px;
                background: $arrow_right;
            }
        }
        .footer{
            height: 100px;
            background: #eee;
            text-align: center;
            padding-top: 20px;
            h3{
                @extend .font16;
                margin-bottom: 20px;
                font-weight: normal;
            }
            ul{
                li{
                    display: inline-block;
                    margin-right: 45px;
                    &:last-child{
                        margin-right: 0;
                    }
                    i{
                        display: inline-block;
                        height: 20px;
                        width: 23px;
                        margin-right: 5px;
                        vertical-align: text-top;
                    }
                    .qq{
                        width: 20px;
                        background: $qq;
                    }
                    .wx{
                        background: $wx;
                    }
                    .phone{
                        width: 13px;
                        background: $phone;
                    }
                }
            }
        }
    }
    #jobIntentionInfo {
        .modal-content {
            border-radius: 0;
            border: none;
        }
        .pt {
            padding: 16px 37px;
            padding-right: 44px;
            border-bottom: 1px solid #e5e5e5;
            word-break: break-all;
        }
        .zy_VideoDemo {
            .close{
                width: 40px;
                height: 40px;
                top:8px;
            }
        }
        .onejiongroup {
            padding: 20px 40px;
            .ptitle {
                background: #F5F5F5;
                padding: 4px 8px;
            }
            table {
                width: 100%;
                td{ padding: 8px 0;}
                .t{ text-align: right; color: #333333; padding-right: 30px;}
            }
        }
        .errorInfo{
            background: #F2DEDE;
            height: 30px;
            line-height: 30px;
            @extend .textC;
            color: #FF725F;
            display: none;
        }
        .statisticsRadio {
            color:#666666;
            font-weight:400;
            margin-right:30px;
            cursor:pointer;
            & > input{
                display:none;
            }
            & > i{
                display:inline-block;
                width:16px;
                height:16px;
                vertical-align:-4px;
                background:url(../images/lps4/userRadioBG.png) 0 0px no-repeat;
                margin-right:10px;
            }
            &.rH > i {
                background-position:0 -88px;
            }
        }
        .job-city {
            position: relative;
        }
        .city-container {
            display: none;
            position: absolute;
            top: 42px;
            left: 0;
            border: 1px solid #d2d2d2;
            background-color: #fff;
            width: 330px;
            overflow: hidden;
        }
        .city-tab {
            height: 38px;
            background-color: #f0fbf9;
            margin: 0 -1px;
            li {
                width: 110px;
                @extend .textC;
                @extend .fl;
                color: #b3b3b3;
                line-height: 38px;
                cursor: pointer;
            }
            .select {
                position: relative;
                color: #333;
                &:after {
                    content: '';
                    display: block;
                    width: 100%;
                    position: absolute;
                    left: 0;
                    bottom: 0;
                    border-top: 2px solid #68d5c3;
                }
            }
        }
        .city-wrap {
            padding-bottom: 4px;
            width: 330px;
            margin: 0 -1px;
            height: 133px;
            overflow-y: scroll;
            overflow-x: hidden;
        }
        .city-cont {
            padding: 20px;
            padding-right: 0;
            display: none;
            font-size: 0;

            span {
                @extend .vam;
                margin-right:16px;
                cursor: pointer;
                #white-space:nowrap;
                line-height: 30px;
                @extend .font14;                
            }
        }
        .city-cont.first {
            display: block;
        }
    }
    .jobIntentionInfoTxt {
        width: 246px;
        height: 34px;
        line-height: 34px;
        padding: 0 5px;
        border: 1px solid #D2D2D2;
        &:focus {
            border-color: #69d5c2;
        }
    }
    .jobIntentionInfoBtn {
        width: 240px;
        height: 40px;
        line-height: 40px;
        @extend .font14;
        @extend .textC;
        color: #ffffff;
        background: #69D5C2;
        border-radius: 2px;
    }
    #MyTaskScore {
        .zy_newclose{
            background:url(../images/lps4/close.png) center center no-repeat;
            font-size: 0;
            height: 17px;
            width: 17px;
            position: absolute;
            top: 20px;
            right: 20px;
            z-index:30;
        }
        .zy_mysuccess {
            width:340px;
            background:#ffffff;
            padding-top:80px;
            .fen {
                padding:10px 0;
                input{
                    vertical-align:-4px;
                    margin-right:10px;
                }
            }
        }
        .zy_mysuccess_topI {
            position:absolute;
            width:148px;
            left:50%;
            margin-left:-74px;
            top:-100px;
        }
        .zy_mysuccess_scorers {
            margin:0 auto;
            width:290px;
            & > div {
                width:140px;
                height:80px;
                background:#fff9df;
                @extend .vam;
            }
        }
        .zy_mytask_foot {
            width:100%;
            background:#f5f5f5;
            padding:20px 0;
            .a2 {
                width:200px;
                line-height:40px;
                background:#5ecfba;
                border-radius:3px;
                display:inline-block;
                &:hover{
                    color:#ffffff;
                }
            }
        }
    }
    #yueke-success,#yueke-over,#yueke-last-step,#ovo-service,#open-service,#open-service {
        .modal-content {
            padding-left: 0;
            padding-right: 0;
        }
        .yueke-box {
            width: 100%;
            padding-bottom: 38px;
            margin-bottom: 20px;
            border-bottom: 1px solid #e5e5e5;
            &:before {
                content: '';                
                background: $bg3;
            }
        }
    }
    #yueke-success {        
        .yueke-box {
            &:before, h4 {
                @extend .vam;
            }
            &:before {
                content: '';
                width: 85px;
                height: 85px;
                margin-right: 20px;
                background: $bg3;
                background-position: -66px 0;
            }
            h4 {
                text-align: left;
                line-height: 36px;
            }
            p {                
                @extend .font24;
                color: #21ce8f;
            }
            span {
                @extend .font14;
                color: #878787;
            }
        }
    }
    #yueke-over {
        .modal-content {
            padding-top: 36px;
            padding-bottom: 25px;
        }
        .yueke-box {
            &:before, h4 {
                display: block;
            }
            &:before {
                content: '';
                width: 91px;
                height: 86px;
                margin: 0 auto 20px;
                background-position: -66px -95px;
            }
            h4 {
                @extend .textC;
                @extend .font14;
                color: #21ce8f;
            }
        }
    }
    #yueke-last-step, #ovo-service,#open-service {
        .modal-content {
            padding: 0;
            padding-bottom: 25px;
        }
        h4 {
            line-height: 102px;
            @extend .textC;
            @extend .font24;
            @extend .bold;
            color: #464646;
            border-bottom: 1px solid #e5e5e5;
            background: $bg2;
            background-position: center 0;
        }
    }
    #yueke-last-step {        
        .yueke-box {
            padding-left: 165px;
            padding-top: 45px;
            text-align: left;
            label {
                display: block;
                @extend .font18;
                color: #464646;
                margin-bottom: 10px;
                margin-left: -16px;
            }
            .phone-num, .verification, .advisor {
                height: 40px;
                @extend .font14;
            }
            .phone-num, .verification {                 
                border-radius: 4px;
                background: #e5e5e5;
                padding-left: 10px;                
            }            
            .advisor, .phone-num {
                width: 294px;
            }
            .advisor {
                line-height: 40px;
            }
            .phone-num {
                margin-bottom: 24px;
            }
            .verification {
                width: 196px;
                margin-right: 8px;
            }
            .verification-box {
                margin-top: 30px;
                position: relative;
            }
            .send-verif {
                @extend .vam;
                width: 88px;
                height: 40px;
                line-height: 38px;
                border-radius: 4px;                
                border: 1px solid #27dd9b;
                color: #27dd9b;
                text-align: center;
                position: absolute;
                left: 204px;
                top: 0;
                @extend .font14;
            }
            .send {
                border: 1px solid #999;
                background: #eaeaea;
                color: #999;
            }
        }
        .tips-box {
            width: 350px;
            display: inline-block;
            position: relative;
        }
        .tips {
            position: absolute;
            left: 0;
            top: -40px;
            color: #fff;
            font-size: 12px;
            line-height: 32px;
            background-color: #ff9600;
            border-radius: 100px;
            padding: 0 15px;
            box-sizing: border-box;
            &:after {
                content: '';
                position: absolute;
                bottom: -12px;
                left: 25px;
                border-top: 6px solid rgba(255,150,0,1);
                border-right: 4px solid transparent;
                border-bottom: 6px solid transparent;
                border-left: 4px solid transparent;
            }
        }
    }
    #ovo-service {
        .yueke-box {
            padding-bottom: 0;
            p {
                @extend .font14;
                @extend .textC;
                color: #464646;
                padding: 35px 32px 30px;
                line-height: 36px;
                border-bottom: 1px solid #e5e5e5;
            }
        }
        span {
            @extend .font12;
            @extend .vam;
            color: #ff8400;
            padding: 22px 11px;
            &:before {
                content: '';
                display: block;
                background: $bg2;
                margin: 0 auto;
                width: 28px;
                height: 28px;
                margin-bottom: 10px;
            }
        }
        .icon-1 {
            &:before {
                background-position: 0 -161px;
            }
        }
        .icon-2 {
            &:before {
                background-position: 0 -199px;
            }
        }
        .icon-4 {
            &:before {
                background-position: 0 -271px;
            }
        }
        .icon-5 {
            &:before {
                background-position: 0 -309px;
            }
        }
        .icon-6 {
            &:before {
                background-position: 0 -349px;
            }
        }
    }
    #open-service {
        h4 {
            background: none;
            border-bottom: none;
        }
        .yueke-box {
            border-bottom: none;
            padding-bottom: 0;
        }
    }
    .perfect-my-resume{
        padding: 46px 0 36px;
        overflow: hidden;
        border-top: 4px solid #27dd9b;
        font-size: 18px;
        p{height: 257px;}
        strong{
            color: #3a3a3a;
            font-weight: normal;
            display: block;
            margin: 30px 0 22px;
        }
        a{
            width: 200px;
            height: 60px;
            line-height: 60px;
            background: #69d5c2;
            text-align: center;
            color: #fff;
            border-radius: 5px;
            margin: 0 auto;
            display: block;
            &:hover{
                background: #49d2ba;
            }
        }
    }
}
@import 'feedbackstyle';
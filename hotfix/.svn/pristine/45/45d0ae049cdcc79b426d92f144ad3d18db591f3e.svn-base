/*重置样式*/
*{margin: 0;padding: 0;}
ul,ol,li{list-style: none;}
input[type="submit"],input[type="text"],input[type="reset"],input[type="search"],input[type="password"],input[type="button"],button,textarea{-webkit-appearance: none;background: none;border: 0;outline: none;}
ul,li,ol,li{list-style: none;}
strong,em,span,a{display: block;}
a{text-decoration: none;}
strong,h1,h2,h3,h4,h5,h6{font-weight: normal;}
em{font-style: normal;}
/*自定义样式*/
body{background:#face55;font-size: 14px;color: #666;font-family: "微软雅黑";}
/*公共样式*/
.share-btn li a,.hd-infos span:before,.closed,.coupon-get .hd-open:before,.register-box:before,.coupon-course li{background: url(/static/images/add_invited_160408/icon.png) no-repeat;}
.hd-infos span:before,.coupon-get .hd-open:before,.register-box:before,.coupon-course figcaption:before,.coupon-course figcaption:after{content: "";display: inline-block;vertical-align: middle;}
.share-box p span,.hd-infos-box .content .closed,.login-body li input,.login-body .login-btn button{border-radius: 3px;-moz-border-radius:3px;-webkit-border-radius:3px;}

/*****邀请页*****/
.containers{min-width:1000px;overflow: hidden;background: url(/static/images/add_invited_160408/bg.png) no-repeat center top;margin: 0 auto;}
.banner{width: 880px;height: 417px;margin: 0 auto;}
/*分享链接*/
.share-box{width:510px;height: 170px;background: #fff;margin: 0 auto;border-radius: 10px 10px 0 0;-moz-border-radius: 10px 10px 0 0;-webkit-border-radius: 10px 10px 0 0;padding: 40px 50px;}
.share-box p{margin-top:10px;overflow: hidden;}
.share-box p span{float: left;height: 38px;border: 1px solid transparent;}
.share-box .txt{width:388px;padding-left:10px;border-color:#ccc;}
.share-box input{width: 100%;height: 100%;font-family: "微软雅黑";font-size: 14px;}
.share-box .txt input{color: #666;}
.share-box .btn{background: #5ecfba;width:98px;margin-left:10px;}
.share-box .btn input{color: #fff;cursor: pointer;}
.share-box .btn:hover,.hd-infos-box .content .closed:hover,.login-body .login-btn button:hover,.register-box button:hover{background: #59b2a1;}
.share-btn{margin-top:20px;}
.share-btn ul{height: 52px;margin:14px 0 0 -20px;}
.share-btn li{float: left;margin-left: 20px;}
.share-btn .weixin{position: relative;}
.share-btn li a{width:52px;height: 52px;text-indent: -99999px;cursor: pointer;}
.share-btn .qq a{background-position: 0 -92px;}
.share-btn .sina a{background-position: 0 -144px;}
.share-btn .weixin a{background-position: 0 -196px;}
.share-btn .q-zone a{background-position: 0 -248px;}
.share-btn li a:hover{opacity: 0.8;filter:alpha(opacity=80);}

.wx-pic{width: 260px;height: 120px;padding: 10px;background: #fff;box-shadow:0 0 10px rgba(0,0,0,.2);position: absolute;top:-145px;left:5px;display: none;}
.wx-pic p{width:95px;height:95px;float: left;}
.wx-pic p img{width: 100%;}
.wx-pic span{margin: 20px 0 0 105px;}

/*活动说明*/
.hd-infos{width:610px;border-bottom:50px solid #efede8;border-right: 186px solid transparent;border-left: 186px solid transparent;margin: 0 auto;position: relative;}
.hd-infos>span{width:610px;line-height:50px;color:#999;text-align: center;position: absolute;cursor: pointer;}
.hd-infos>span:before{width: 22px;height: 22px;background-position: -60px -33px;margin-right: 5px;}

.bg-box{width: 100%;height: 100%;position: fixed;left: 0;top: 0;background: rgba(0,0,0,0.6);z-index: 100;display: none;}
.hd-infos-box{width:560px;height: 500px;position: fixed;left: 50%;top: 50%;margin: -250px 0 0 -280px;background: #fff;z-index: 101;display: none;}
.hd-infos-box .title{height:53px;padding: 2px 20px 0;border-bottom: 1px solid #e5e5e5;}
.hd-infos-box .title strong{font-size: 18px;color: #1bbc9b;line-height: 54px;}
.closed{width:19px;height:19px;background-position:-36px -53px;float: right;text-indent: -99999px;margin-top: 16px;cursor: pointer;}
.hd-infos-box .content{padding: 0 28px;}
.hd-infos-box li,.hd-infos-box p{line-height: 24px;margin-top:20px;}
.hd-infos-box .content em{display: inline-block;}
.hd-infos-box .content .closed{width: 120px;height: 40px;line-height: 40px;color: #fff;font-weight: bold;text-align: center;background: #5ecfba;margin: 40px auto 0;float:none;text-indent: 0;}

/*步骤*/
.steps{width:870px;margin:0 auto;padding: 50px 65px;overflow: hidden;background: #fff;border-radius: 0 0 10px 10px;-moz-border-radius: 0 0 10px 10px;-webkit-border-radius: 0 0 10px 10px;margin-bottom:67px;}
.steps li{overflow: hidden;}
.steps li{width:290px;float: left;}

/*登录框*/
.login-box{width: 400px;max-height: 374px;position: fixed;left: 50%;top: 50%;margin: -187px 0 0 -200px;background: #fff;padding-bottom:29px;display: none;z-index: 102;}
.login-box .title{padding: 15px;height: 25px;border-bottom: 1px solid #e5e5e5;}
.login-box .title strong{line-height: 25px;color: #5ecfba;font-size: 18px;}
.login-box .title .closed{margin-top: 3px;}
.login-body{padding: 0 32px;overflow: hidden;}
.login-body li{height:40px;margin-top: 15px;}
.login-body li input{width:312px;font-family: "微软雅黑";font-size: 14px;color: #555;height:28px;padding:6px 12px;border: 1px solid #ccc;}
.login-body li input:focus{border-color: #5ecfba;}
.login-body .good-tips{margin: 15px 0;}
.Forgetpsw{float: right;}
.login-body .good-tips a{display: inline-block;color: #5ecfba;}
.login-body .login-btn{height: 40px;text-align: center;}
.login-body .login-btn button{width:100%;height: 40px;background: #5ecfba;cursor: pointer;font-size: 16px;color: #fff;font-family: "微软雅黑";}
.login-body .tips-error{display: none;margin-top: 15px;padding: 5px 0;text-align: center;background: #f2dede;}
.login-body .otherLogin{margin-top: 19px;}
.login-body .otherLogin span{color: #999;display: inline-block;}
.login-body .otherLogin a{float: right; width: 20px; height: 20px; background: url(/static/images/add_invited_160408/login_img.png) no-repeat; vertical-align: middle; margin-left: 15px;cursor: pointer;}
.login-body .otherLogin a.qq{ background-position: 0 0;}
.login-body .otherLogin a.qq:hover{ background-position: 0 -20px;}
.login-body .otherLogin a.wx{ background-position: -20px 0;}
.login-body .otherLogin a.wx:hover{ background-position: -20px -20px;}
.login-body .otherLogin a.wb{ background-position: -40px 0;}
.login-body .otherLogin a.wb:hover{ background-position: -40px -20px;}

/*******被邀请页*******/
.containers2{min-width: 1160px;max-width: 1920px;}
.containers h1{width: 722px;height:44px;margin: 0 auto;padding: 28px;padding-right: 0;}
/*领取优惠券*/
.coupons-content{background: url(/static/images/add_invited_160408/coupons_bg.png) no-repeat center 57px;overflow: hidden;}
.coupon-price{text-align: center;margin-top: 85px;}
.coupon-price strong{font-size: 70px;color: #e84637;font-weight: bold;}
.coupon-price span{font-size: 28px;color: #be9136;font-weight: bold;}
.coupon-get{width:750px;overflow: hidden;background: #ea5244;margin: 180px auto 0;padding-bottom:  20px;}
.coupon-get .get-btn,.back_top a{width: 250px;height: 66px;background: #face55;color:#a87712;font-size: 26px;font-weight:bold;line-height: 66px;text-align: center;margin:0 auto;border-radius: 5px;-moz-border-radius:5px;-webkit-border-radius:5px;box-shadow: 0 4px 0 0 #d9a002;cursor: pointer;}
.coupon-get .hd-open{width:90px;margin: 60px auto 0;text-align: center;cursor: pointer;display: block;color: #ffc8c8;}
.coupon-get .hd-open:before{width: 22px;height: 22px;background-position: -60px -61px;margin-right: 5px;}
/*注册*/
.register-box{width:402px;overflow: hidden;margin: 0 auto;display: none;}
.register-box:before{width: 32px;height: 22px;background-position: 0 -56px;display: block;margin: 44px auto 0;}
.register-box .form-box{width:322px;height:auto;padding: 20px 40px;background: #fff;margin-top:-1px;}
.register-box .inputs{border: 1px solid #d8d8d8;padding: 0 15px;margin-top: 20px;}
.register-box .inputs input{width:100%;height: 34px;font-family: "微软雅黑";color: #999;font-size: 12px;}
.register-box input:-webkit-autofill {-webkit-box-shadow: 0 0 0px 1000px white inset !important;}
.register-box .validation{font-size: 12px;margin-top: 20px;}
.register-box button{width:100%;font-size: 16px;height: 50px;line-height: 50px;color: #fff;background: #5ecfbb;font-family: "微软雅黑";margin-top: 20px;cursor: pointer;}
.register-box .tips{padding: 15px 0 10px;text-align: center;}

/*课程*/
.coupon-course{overflow: hidden;background: url(/static/images/add_invited_160408/coupon_img1.jpg) no-repeat center top;background-size: cover;padding: 110px 0;}
.coupon-course figure,.coupon-course .slogan,.coupon-course ul{width: 750px;margin: 0 auto;color: #fff;}
.coupon-course figure{width: 622px;font-weight: bold;}
.coupon-course figure span{display: inline-block;margin: 0 6px;}
.coupon-course figure p{padding:16px 0 25px;border-bottom: 1px solid #afaaa9;font-size: 18px;}
.coupon-course figcaption span{margin: 0 31px;font-size: 20px;}
.coupon-course figcaption:before,.coupon-course figcaption:after{height: 1px;width: 240px;background: #afaaa9;}
.coupon-course .slogan{text-align: center;margin-top: 45px;}
.coupon-course .slogan span{font-size: 70px;}
.coupon-course .slogan strong{font-size: 100px;}
.coupon-course ul{width:724px;overflow: hidden;margin: 42px auto 0;padding-left:26px;}
.coupon-course li{float: left;font-size: 26px;margin-left: 60px;background-position:-58px 4px;padding-left: 42px;}
/*介绍*/
.coupon-desc{overflow: hidden;padding: 50px 0 78px;background: #fff;}
.containers2 .titles,.coupon-desc dl,.coupon-student ul,.coupon-protocol .detail{width: 1160px;margin: 0 auto;text-align: center;}
.containers2 .titles h3{font-size: 40px;color: #333;font-weight: bold;}
.containers2 .titles em{display: inline-block;color: #5ecfba;}
.containers2 .titles span{font-size: 24px;margin-top: 10px;}
.coupon-desc dl{overflow: hidden;margin-top: 72px;}
.coupon-desc dt{width:670px;height:380px;float: right;}
.coupon-desc dd{margin: 70px 670px 0 92px;text-align: left;}
.coupon-desc dd strong{font-size: 40px;color: #5ecfba;}
.coupon-desc dd span{display: inline-block;border-bottom:2px solid #ccc;padding-bottom: 18px;}
.coupon-desc dd ul{margin-top:30px;}
.coupon-desc dd li{font-size: 18px;line-height: 32px;}

.section5{min-width: 1160px;}
.bxslider2 li>img{width: auto;}

/*学员*/
.coupon-student{padding: 145px 0 55px;background: #fff;}
.coupon-student ul{margin-top: 45px;overflow: hidden;}
.coupon-student li{float: left;width: 290px;padding-top:10px;}
.coupon-student li p,.coupon-student li img{width: 140px;height: 140px;border-radius: 100%;-moz-border-radius:100%;-webkit-border-radius:100%;}
.coupon-student li p{margin: 0 auto;}
.coupon-student li img{-webkit-transition:all 0.3s ease-out 0s;-moz-transition:all 0.3s ease-out 0s;-o-transition:all 0.3s ease-out 0s;transition:all 0.3s ease-out 0s;}
.coupon-student li a:hover img{transform: scale(1.1);-ms-transform: scale(1.1);-webkit-transform: scale(1.1);-o-transform: scale(1.1);-moz-transform: scale(1.1);}
.coupon-student li strong{font-size: 24px;color: #333;margin-top: 15px;}
.coupon-student li span{color: #666;margin-top: 12px;}
.coupon-student li em{color: #333;margin-top: 5px;}

/*协议*/
.coupon-protocol{padding: 5px 0 50px;overflow: hidden;background: #fff;}
.coupon-protocol .detail{height:300px;position: relative;background: url(/static/images/add_invited_160408/coupon_xybg.png) no-repeat right bottom #333;margin-top: 68px;}
.coupon-protocol p{position: absolute;left: 100px;top: -67px;}
.coupon-protocol strong,.coupon-protocol span{margin-left:500px;text-align: left;}
.coupon-protocol strong{font-size: 40px;color: #ccc;font-weight: bold;padding-top: 110px;}
.coupon-protocol span{font-size: 22px;color: #fff;margin-top: 5px;}

.back_top{height: 170px;background: #ea5244;overflow: hidden;}
.back_top a{width: 362px;margin-top: 50px;}






@media only screen and (min-width: 1500px){	
	.coupon-course{padding-bottom: 15%;}
}
@media only screen and (min-width: 1600px){
	.coupons-content{background-size: 100%;}
	.coupon-price{margin-top: 5.5%;}
	.coupon-get{width:46.875%;margin-top: 12%;}
}

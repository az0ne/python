/*我的提问*/

.personalInterlocutionQues>div {
    position: relative;
}

.personalInterlocutionQues>div>ul {
	margin: 30px 0;
	height: 0;
	overflow: hidden;
	osition: relative;
	-webkit-transition: height 0.3s;
	-moz-transition: height 0.3s;
	-o-transition: height 0.3s;
	transition: height 0.3s;
}


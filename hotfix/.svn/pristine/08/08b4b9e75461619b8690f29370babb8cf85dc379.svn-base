.course_appointment { background: -webkit-linear-gradient(#dce8f2, #e2e2ea); background: -o-linear-gradient(#dce8f2, #e2e2ea); background: -moz-linear-gradient(#dce8f2, #e2e2ea); background: linear-gradient(#dce8f2, #e2e2ea); }
.course_appointment .head-container { position: fixed; left: 0; top: 0; z-index: 200; }
.course_appointment .main { position: relative; }
.course_appointment .main-container { width: 1160px; margin: 65px auto 0; background: url(../images/appointment/appo_3.png) 0 0 no-repeat; }
.course_appointment .main-container:before, .course_appointment .main-container:after { content: ''; display: table; clear: both; }
.course_appointment .main-container .left { width: 834px; float: left; margin-left: -77px; box-sizing: border-box; }
.course_appointment .main-container .right { width: 393px; float: right; padding-top: 93px; box-sizing: border-box; position: relative; }
.course_appointment .chat-window, .course_appointment .teacher-window { width: 100%; height: 758px; box-sizing: border-box; }
.course_appointment .chat-window .img-box, .course_appointment .teacher-window .img-box { width: 112px; height: 112px; border-radius: 50%; overflow: hidden; }
.course_appointment .chat-window .img-box img, .course_appointment .teacher-window .img-box img { max-width: 100%; width: 100%; }
.course_appointment .chat-window { padding-top: 180px; padding-left: 106px; background: url(../images/appointment/appo_1.png) 0 0 no-repeat; }
.course_appointment .teacher-window { padding-top: 138px; padding-left: 99px; padding-right: 137px; background: url(../images/appointment/appo_2.png) 0 0 no-repeat; }
.course_appointment .teacher-window .img-box { float: left; }
.course_appointment .teach-intro { float: right; width: 468px; height: 560px; margin-top: -26px; padding-top: 40px; padding-left: 20px; padding-right: 20px; box-sizing: border-box; }
.course_appointment .teach-intro h2, .course_appointment .teach-intro h3 { color: #3d3d3d; }
.course_appointment .teach-intro h2 small { color: #9d9d9d; margin-left: 10px; }
.course_appointment .teach-intro h2 + .intro-desc { padding-bottom: 24px; border-bottom: 1px solid #e7e7e7; }
.course_appointment .teach-intro .intro-desc { margin: 24px auto; color: #757575; line-height: 24px; }
.course_appointment .swiper-container { width: 400px; height: 140px; }
.course_appointment .swiper-button-prev, .course_appointment .swiper-button-next { width: 36px; height: 36px; background: url(../images/appointment/appo_4.png) 0 0 no-repeat; margin-top: -18px; }
.course_appointment .swiper-button-prev { background-position: 0 -148px; }
.course_appointment .swiper-button-prev:hover { background-position: -46px -148px; }
.course_appointment .swiper-button-next { background-position: -92px -148px; }
.course_appointment .swiper-button-next:hover { background-position: -138px -148px; }
.course_appointment .swiper-slide img { width: 266px; height: 140px; max-width: 100%; max-height: 100%; -ms-transform: translate(-50%, -50%); -webkit-transform: translate(-50%, -50%); -moz-transform: translate(-50%, -50%); transform: translate(-50%, -50%); position: absolute; left: 50%; top: 50%; }
.course_appointment .appointment-form { height: 450px; text-align: right; position: fixed; top: 158px; width: 393px; z-index: 10; }
.course_appointment .appointment-form h2 { height: 65px; background: url(../images/appointment/appo_4.png) 0 0 no-repeat; background-position: 12px 0; }
.course_appointment .appointment-form p { color: #ff8400; margin: 20px auto; }
.course_appointment .appointment-form .start-time { color: #454546; line-height: 42px; margin-bottom: 35px; }
.course_appointment .appointment-form strong { font-weight: 400; color: #000; font-size: 14px; margin-left: 10px; }
.course_appointment .appointment-form strong span { display: inline-block; font-size: 18px; color: #fff; text-align: center; width: 40px; height: 35px; line-height: 35px; background: url(../images/appointment/appo_4.png) 0 0 no-repeat; background-position: 0 -79px; }
.course_appointment .appointment-form .phone-num, .course_appointment .appointment-form .verification, .course_appointment .appointment-form .just-appo-btn { width: 350px; height: 67px; border-radius: 5px; font-size: 18px; }
.course_appointment .appointment-form .phone-num, .course_appointment .appointment-form .verification { background-color: #fff; padding: 20px; box-sizing: border-box; border-left: 2px solid #f0f0f0; border-top: 2px solid #f0f0f0; box-shadow: 26px 0 70px rgba(0, 0, 0, 0.15); }
.course_appointment .appointment-form .verification-box { margin: 10px auto 30px; position: relative; }
.course_appointment .appointment-form .verification { padding-right: 104px; }
.course_appointment .appointment-form .just-appo-btn { display: inline-block; background: -webkit-linear-gradient(#1fd783, #1fd06b); background: -o-linear-gradient(#1fd783, #1fd06b); background: -moz-linear-gradient(#1fd783, #1fd06b); background: linear-gradient(#1fd783, #1fd06b); text-align: center; line-height: 67px; color: #fff; font-weight: 600; text-shadow: 2px 2px 4px rgba(0, 0, 0, 0.11); box-shadow: 26px 0 70px rgba(0, 0, 0, 0.15); }
.course_appointment .appointment-form .send-verif { display: block; width: 90px; height: 36px; line-height: 36px; border-radius: 5px; color: #fff; text-align: center; font-size: 12px; position: absolute; right: 10px; top: 16px; background: -webkit-linear-gradient(#1fd783, #1fd06b); background: -o-linear-gradient(#1fd783, #1fd06b); background: -moz-linear-gradient(#1fd783, #1fd06b); background: linear-gradient(#1fd783, #1fd06b); }
.course_appointment .appointment-form .send { background: #eaeaea; }
.course_appointment .tips-box { width: 350px; display: inline-block; position: relative; }
.course_appointment .tips { position: absolute; left: 0; top: -40px; color: #fff; font-size: 12px; line-height: 32px; background-color: #ff9600; border-radius: 100px; padding: 0 15px; box-sizing: border-box; }
.course_appointment .tips:after { content: ''; position: absolute; bottom: -12px; left: 25px; border-top: 6px solid #ff9600; border-right: 4px solid transparent; border-bottom: 6px solid transparent; border-left: 4px solid transparent; }
.course_appointment .appo-success-modal { position: absolute; left: 0; top: 0; width: 100%; height: 100%; background-color: rgba(255, 255, 255, 0.6); z-index: -100; opacity: 0; filter: Alpha(Opacity=0); overflow: hidden; }
.course_appointment .appo-success { width: 728px; height: 457px; margin: -500px auto 0; z-index: 150; background-color: #fff; padding-top: 5px; box-sizing: border-box; box-shadow: 2px 8px 21px rgba(0, 0, 0, 0.1); }
.course_appointment .appo-success .left, .course_appointment .appo-success .right { float: left; height: 350px; }
.course_appointment .appo-success .left { width: 268px; }
.course_appointment .appo-success .right { width: 460px; border-left: 1px solid #e7e7e7; padding: 65px 0 0 40px; box-sizing: border-box; }
.course_appointment .appo-success .right h1 { padding-left: 130px; padding-top: 30px; box-sizing: border-box; height: 107px; background: url(../images/appointment/appo_4.png) 0 0 no-repeat; line-height: 30px; background-position: -289px -76px; margin-bottom: 30px; }
.course_appointment .appo-success .right p { color: #444; }
.course_appointment .appo-success .right span { color: #717171; }
.course_appointment .appo-success .qr-code { width: 177px; height: 234px; margin: 58px auto; border: 1px solid #e7e7e7; padding: 5px; box-sizing: border-box; font-size: 12px; }
.course_appointment .appo-success .qr-code img { margin-bottom: 8px; max-width: 100%; }
.course_appointment .appo-success .qr-code p { text-align: center; line-height: 22px; }
.course_appointment .appo-success .bottom { width: 100%; clear: both; border-top: 1px solid #e7e7e7; padding-left: 46px; padding-top: 35px; box-sizing: border-box; }
.course_appointment .appo-success .appo-success-close { display: block; width: 176px; height: 40px; line-height: 40px; text-align: center; font-size: 14px; color: #aeaeae; border: 1px solid #aeaeae; border-radius: 100px; }
.course_appointment .appo-success .go-live { display: block; width: 223px; height: 67px; margin-left: 130px; border-radius: 5px; background: -webkit-linear-gradient(#1fd783, #1fd06b); background: -o-linear-gradient(#1fd783, #1fd06b); background: -moz-linear-gradient(#1fd783, #1fd06b); background: linear-gradient(#1fd783, #1fd06b); text-align: center; line-height: 67px; color: #fff; font-size: 18px; font-weight: 600; text-shadow: 2px 2px 4px rgba(0, 0, 0, 0.11); box-shadow: 26px 0 70px rgba(0, 0, 0, 0.15); }

*{margin: 0;padding: 0;}
body{padding: 0;margin: 0;font-family: "microsoft yahei";font-size: 16px;color: #666;line-height: 1.6;background: #f5f5f5;}
ul,li,ol{list-style: none;}
.microClass a{text-decoration: none;display: inline-block;}
.microClass em{font-style: normal;}
.microClass img{display:block;width: 100%;}
.microClass strong,.microClass span,.microClass em{display: inline-block;}
.microClass strong,.microClass h3{font-weight: normal;}

.microClass{width: 100%;overflow: hidden;}
.microClass .ClassList strong,.microClassVideo .shareBack,.microClassChat .chatBox,.microClassShareOther .jiathis_style,.microClassChat .tips{box-sizing:border-box;-moz-box-sizing:border-box;-webkit-box-sizing:border-box;}
.microClassVideo em:after,.microClass .shareBack a,.microClassShareOther .closed,.microClassShareOther .jiathis_style .jtico,.microClass .wikeAnnounced em,.microClassChat .tips:before,.microClassChat .user span,.microClassChat .userx span{background-image:url(/static/images/wapwike/microCourseIcon.png);background-repeat: no-repeat;}

/*微课列表*/
.microClass .ClassList{padding: 0 1.875rem 1.875rem;overflow: hidden;}
.microClass .ClassList li{background: #fff;margin-top: 1.875rem;}
.microClass .ClassList li a{display: block;}
.microClass .ClassList p{position: relative;height:auto;min-height: 300px;}
.microClass .ClassList strong{width:100%;height:3.125rem;line-height:2.1;color:#fff;font-size: 1.5rem;position: absolute;left:0;bottom:0;z-index:9;display: block;background: rgba(0,0,0,.3);padding: 0 1rem;}
.microClass .ClassList h3{font-size: 2.125rem;margin:.75rem 1.25rem 0;color: #333;}
.microClass .ClassList span{color: #999;font-size: 1.625rem;margin:0 1.25rem 1.375rem;}
.microClass .wikeAnnounced em{width: 8.3125rem;height: 3.5625rem;display: block;font-size:1.625rem;color:#fff;line-height:2.9375rem;padding-left:1.375rem;background-position: 0 -539px;position: absolute;top: .625rem;left: -.625rem;z-index: 99;}

/*微课详情*/
.microClassVideo{position: relative;overflow: hidden;}
.microClassVideo video{width: 100%;display: block;}
.microClassVideo em{display: none;overflow: hidden;}
.microClassVideo em:before,.microClassVideo em:after{content:"";display: block;position: absolute;}
.microClassVideo em:before{width: 100%;height: 100%;background: rgba(34,25,4,.5);}
.microClassVideo em:after{width:182px;height:182px;right: 0;top: 0;bottom: 0;left: 0;margin: auto;}

.microClass .shareBack{width: 100%;height: 4.8125rem;border-bottom: 2px solid #dce0e0;}
.microClass .shareBack a{width: 4.5rem;height: 4.5em;text-indent: -9999rem;margin-top: 2px;}
.microClass .shareBack a:first-child{float: left;background-position: 0 -181px;margin-left: .5rem;}
.microClass .shareBack a:last-child{float: right;background-position: -71px -181px;margin-right: .5rem;}
/*.microClassVideo section{height: 5.625rem;line-height: 5.625rem;background: #fff;padding: 0 1.875rem;}*/
/*.microClassVideo section strong{float: left;font-size: 1.5rem;color: #999;}*/
/*.microClassVideo section h2{font-size: 1.875rem;font-weight: normal;}*/

.microClassCount{height: 6.25rem;background: #fff;border-bottom: 1px solid #e9e9e9;margin-top: 1.25rem;padding: 0 1.875rem;}
.microClassCount ul{float: left;margin: 1.5625rem 0 0 1.25rem;}
.microClassCount li{width: 3.125rem;height: 3.125rem;border-radius: 50%;overflow: hidden;box-shadow: 0 0 0 .3125rem #fff;float: left;background: #ccc;margin-left: -1.1875rem;}
.microClassCount span{overflow:hidden;height:6.25rem;margin-left: 20rem;display: block;font-size: 1.875rem;line-height: 6.25rem;letter-spacing: -1px;}
.microClassCount span strong{max-width: 115px;display: inline-block;text-overflow: ellipsis;white-space: nowrap;overflow: hidden;vertical-align: bottom;}

.microClassTab{margin-top: 1.25rem;}
.microClassTab .title{height: 5.5625rem;background: #fff;font-size: 1.875rem;text-align: center;}
.microClassTab .title span{width: 50%;float: left;padding: 1.25rem 0;border-bottom: 1px solid #dce0e0;}
.microClassTab .title span.on{border-bottom: .25rem solid #5ecfba;padding-bottom: 1.0625rem;}
.microClassTab .content>div{display: none;}
.microClassTab .content>div.on{display: block;}
.microClassTabInfo .teacher{padding: 1.875rem 0 2.5rem;background: #fff;overflow: hidden;border-bottom: 2px solid #dce0e0;}
.microClassTabInfo .img{width: 9.375rem;height: 9.375rem;overflow: hidden;border-radius: 50%;margin-left: 1.875rem;float: left;}
.microClassTabInfo .txt{margin: 0 1.875rem 0 13.125rem;overflow: hidden;}
.microClassTabInfo .txt strong{font-size: 2.25rem;color: #333;padding: .25rem 0;}
.microClassTabInfo .txt p{font-size: 1.75rem;display:table;height:1px;}
.microClassTabInfo .abstract{overflow: hidden;border-bottom: 2px solid #dce0e0;}
.microClassTabInfo h3{height: 5.625rem;line-height: 5.625rem;font-size: 2.215rem;font-weight: bold;padding: 0 1.875rem;}
.microClassTabInfo .abstract p{font-size: 1.75rem;display: table;height: 1px;background: #fff;padding: 1.5rem 1.875rem;}
.microClassTabInfo .profession ul{padding: 1.875rem;background: #fff;overflow: hidden;margin-left: -1.875rem;}
.microClassTabInfo .profession li{width: 33.33%;float: left;}
.microClassTabInfo .profession a{margin-left: 1.875rem;display: block;}
.microClassTabInfo .profession strong{color:#333;font-size: 1.5rem;display: block;margin-top: 0.5rem;}

.microClassChat{margin-bottom: 6.25rem;}
.microClassChat .record{width:100%;background:#f5f5f5;padding-top:1.25rem;}
.microClassChat .tips{width:100%;font-size: 1.25rem;height:1px;display:table;color: #999;line-height:1.5;background: #fff9c9;padding:1.125rem 1.875rem;padding-left: 7.5rem;}
.microClassChat .tips:before{width: 1.6875rem;height: 1.5625rem;content: "";display: block;background-position: -162px -444px;float: left;margin:3px 0 0 -2.875rem;}
.microClassChat .ques{padding: 0 1.875rem 1.875rem;overflow: hidden;background: #fff;}
.microClassChat li{overflow: hidden;margin-top: 1.875rem;}
.microClassChat .img{width:4.375rem;height: 4.375rem;border-radius: 50%;overflow: hidden;float: left;}
.microClassChat .txt,.microClassChat .item{margin-left: 5.625rem;color: #666;}
.microClassChat .user,.microClassChat .userx{font-size: 1.5rem;overflow: hidden;font-family: "Microsoft YaHei", "arial";}
.microClassChat .user span,.microClassChat .userx span{float: right;color: #999;background-position: -163px -472px;padding-left: 2rem;color: #999;}
.microClassChat .user .praised,.microClassChat .userx .praised{color:#5ecfba;background-position: -163px -510px;}

.microClassChat .txt p{margin-top: .5rem;}
.microClassChat li p{line-height: 2.5rem;font-size: 1.75rem;height: 1px;display: table;}
.microClassChat .item{background: #eee;border-radius: .625rem;padding:1.5rem 2rem;margin-top: 1.5rem;}
.microClassChat .item:before{width:.25rem;height: 1.75rem;background: #5ecfba;content: "";display: block;float: left;margin: .5rem 0 0 -2rem;}

.microClassChat .chatBox{width:100%;height: 6.1875rem;border-top: 1px solid #c3c3c5;background: #fff;padding: .875rem 1.875rem 0;position: fixed;left: 0;bottom:0;z-index: 9;}
.microClassChat .chatBox .tip{position: absolute;left:0;right:0;top:0;opacity:0;margin:-2.75rem auto 0;;width: 11.5rem;height: 2.375rem;line-height: 2.375rem;font-size: 1.25rem;color: #fff;text-align: center;background: rgba(0,0,0,.5);display: block;-webkit-transition: opacity .3s;-moz-transition: opacity .3s;transition: opacity .3s;}
.microClassChat .sendBtn{float: right;display: block;width: 6.25rem;height: 4.5rem;line-height: 4.5rem;border-radius: .25rem;background: #5ecfba;font-size: 1.875rem;color: #fff;text-align: center;}
.microClassChat .textBox{margin-right: 7.5rem;height:3.375rem;display: block;border: 1px solid #abadb2;border-radius: .375rem;padding:0.5rem 1rem;}
.microClassChat .textBox input{width:100%;height:100%;color: #333;font-size: 1.875rem;font-family: "microsoft yahei";border: 0;}

.microClassChatTip{background: #fff;text-align: center;padding-top: 4.25rem;}
.microClassChatTip p img,.microClassShareWx img{max-width: 100%;width: auto;margin: 0 auto;}
.microClassChatTip strong{font-size: 1.875rem;margin-top: 2rem;margin-bottom: 6.25rem;}

.microClassShare{width: 100%;height: 100%;background: rgba(0,0,0,.4);position: fixed;left: 0;top: 0;z-index: 10000;}
.microClassShareWx{text-align: right;display: none;}
.microClassShareWx img{margin-right: 1.875rem;display: inline-block;}

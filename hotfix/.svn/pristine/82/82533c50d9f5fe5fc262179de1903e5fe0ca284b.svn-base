    <script type='text/javascript'>
        // get header user info of fps
        $(function(){
            $.ajax({
                url: '/lps3/student/ajax/head_user_info/' + {{request.user.id}} + '/',
                type: 'GET',
                dataType: 'Json',
                success: function(data){

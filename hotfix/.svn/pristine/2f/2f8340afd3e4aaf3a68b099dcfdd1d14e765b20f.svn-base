			@include box-shadow(none);
			&:after {
				display: none;
			}
			&:before {
				content: '';
				width: 40px;
				height: 40px;
				display: block;
				margin: 56px auto 16px;
				background-image: url("data:image/png;base64,");
				background-position: center;
				@include borderradius(50%);
				@include box-shadow(2px 2px 3px 0px rgba(0,0,0,0.06));				
			}
			&:hover {
				&:before {
					background-image: url("data:image/png;base64,");
				}
			}
		}
	} 
}


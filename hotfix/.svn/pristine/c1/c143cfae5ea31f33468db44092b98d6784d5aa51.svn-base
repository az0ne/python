@import 'core/reset';
@import 'core/common';
@import 'core/var';
@import 'core/function';

@import 'header';
@import 'toolbar';
@import 'jspPane';
.sprite {
    background: url(../images/course_introduce/sprite.png) 0 0 no-repeat;
}

.course-introduce {
    i, em {
        font-style: normal;
    }
    .section_1 {
        width: 100%;
        .inner {
            width: 850px;
            margin: 0 auto;
            padding-top: 140px;
            padding-bottom: 120px;
            text-align: center;
            .title {
                font-size: 12px;
                color: transparent;
                text-indent: -99999px;
                padding-bottom: 18px;
                margin-bottom: 10px;
            }
            h1 {
                font-size: 48px;
                color: #ffffff;
                margin-bottom: 30px;
                font-weight: 700;
            }
            .desc {
                font-size: 18px;
                color: #c6d1dd;
                line-height: 30px;
                margin-bottom: 30px;
                height: 65px;
                overflow: hidden;
            }
        }
        .row_1 {
            padding-left: 228px;
            box-sizing: border-box;
            text-align: left;
            height: 42px;
            margin-bottom: 10px;
            p, .time-item {
                width: 180px;
                text-align: center;
            }
            p {
                font-size: 12px;
                color: #ffffff;
            }
            .time-item {
                font-size: 18px;
                color: #ffffff;
            }
            .come {
                font-size: 18px;
                line-height: 42px;
            }
        }
        .row_2 {
            font-size: 0;
            margin-bottom: 10px;
            a {
                color: #ffffff;
                font-size: 16px;
                @extend .vam;
            }
            .try_learn, .just_up {
                width: 180px;
                height: 50px;
                line-height: 48px;
                border-radius: 2px;
            }
            .try_learn {
                border: 1px solid #5ecfba;
                background: #5ecfba;
            }
            .just_up {
                border: 1px solid #ffffff;
                margin-left: 40px;
                transition: .5s;
            }
            .just_up:hover {
                background-color: #fff;
                color: #5ecfba;
            }
            .payment {
                line-height: 50px;
                margin-left: 40px;
            }
        }
        .row_3 {
            text-align: left;
            padding-left: 196px;
            span {
                font-size: 14px;
                color: #ffffff;
            }
            i {
                font-weight: bold;
            }
        }
    }
    .section_2 {
        width: 100%;
        background: #f5f5f5;
        .tab_menu {
            width: 100%;
            margin: 0 auto;
            text-align: center;
            background-color: #fff;
            font-size: 0;
            border-bottom: 1px solid #ededed;
            box-shadow: 0 1px 5px rgba(0,0,0,0.1);
            li {
                width: 94px;
                @extend .vam;
                height: 54px;
                line-height: 54px;
                font-size: 16px;
                margin-right: 60px;
                cursor: pointer;
            }
            li:last-child {
                margin-right: 0;
            }
            .active {
                color: #00b38a;position: relative;
                &:after {
                    content:'';
                    width: 100%;
                    border-top: 3px solid #5ecfba;
                    position: absolute;
                    left: 0;
                    bottom: -2px;
                }
            }
        }
        .tab_box {
            width: 100%;
            & > div {
                display: none;
                &:first-child {
                    display: block;
                }
            }
            .row_1 {               
                .inner {
                    @extend .W1160;
                    font-size: 0;
                    padding-top: 60px;
                    padding-bottom: 80px;
                }
            }
        }

        .lesson_intro {
            .row_2, .row_3, .row_4, .row_5 {
                .title, .sub_title {
                    text-align: center;
                }
                .title {
                    font-size: 30px;
                    font-weight: bold;
                    color: #333;
                    margin-bottom: 10px;
                }
                .sub_title {
                    font-size: 16px;
                    color: #666;
                    margin-bottom: 40px;
                }
            }
            .row_1 {
                width: 100%;
                height: 430px;
                .col_l, .col_r {
                    width: 50%;
                    @extend .vam;
                }
                .col_l{
                    padding-left: 120px;
                    p {
                        font-size: 16px;
                        line-height: 28px;
                        width: 420px;
                        color: #666;
                        margin-bottom: 28px;
                    }
                    .person_num {
                        font-size: 0;
                        i, div {
                            @extend .vam; 
                        }
                        i {
                            width: 60px;
                            height: 60px;
                            background: url(../images/course_introduce/sprite.png) 0 0 no-repeat;
                            margin-right: 18px;
                        }
                        div {
                            span {
                                display: block;
                            }
                            span:first-child,em {
                                font-size: 14px;
                                color: #666;
                            }
                            span:last-child {
                                font-size: 30px;
                                color: #333;
                            }
                            em {
                                color: #333;
                            } 
                        }
                    }
                }
                .col_r {
                    padding-left: 40px;
                    p {
                        font-size: 14px;
                        color: #666;                    
                    }
                    .title {
                        margin-bottom: 20px;
                    }
                    .sub_title {
                        margin-bottom: 16px;
                    }
                    ul {
                        margin-bottom: 48px;
                        &:before, &:after {
                            @extend .CLEARFIX;
                        }
                        li {
                            @extend .fl;
                            width: 120px;
                            height: 40px;
                            margin-right: 30px;
                            img {
                                width: 120px;
                                height: 40px;
                                min-width: 100%;
                            }
                        }

                    }
                    strong {
                        @extend .vam;
                        margin-right: 40px;
                        font-weight: 400;
                        span {
                            display: block;
                        }
                        span:first-child {
                            font-size: 18px;
                            color: #333;
                        }
                        span:last-child {
                            font-size: 14px;
                            color: #999;
                        }
                    }
                }
            }
            .row_2 {
                .title_module {
                    background: #fff;
                    width: 100%;
                    height: 240px;
                    padding-top: 62px;
                }
                .tab_nav {
                    font-size: 0;
                    width: 100%;
                    text-align: center;
                    position: relative;
                    li {
                        @extend .vam;
                        height: 74px;
                        margin-right: 30px;
                        padding: 0 15px;
                        z-index: 10;
                        position: relative;
                    }
                    li:last-child {
                        margin-right: 0;
                    }
                    .active {
                        position: relative;
                        em {
                            position: absolute;
                            left: 0;
                            bottom: 0;
                            content: '';
                            width: 100%;
                            border-width: 3px;
                            border-style: solid;
                            border-color: #5ecfba;
                        }
                    }
                    i {
                        display: block;
                        width: 34px;
                        height: 34px;
                        color: #fff;
                        margin: 0 auto;
                        border-radius: 50%;
                        position: relative;
                        font-size: 17px;
                        line-height: 34px;
                        margin-bottom: 10px;
                    }
                    span {
                        font-size: 16px;
                        white-space:nowrap;
                    }
                    .line {
                        height: 0;
                        padding: 0;
                        border-bottom: 1px solid #ededed;
                        position: absolute;
                        top: 17px;
                        z-index: 5;
                    }
                }
                .list_module {
                    width: 100%;
                    height: 610px;
                    background: #f5f5f5;
                    padding: 60px 0;
                    overflow: hidden;
                    .inner {
                        @extend .W1160;
                        height: 100%;                        
                        overflow: visible !important;
                    }
                    .jspContainer {
                        overflow: visible;
                    }
                    .jspHorizontalBar {
                        bottom: 124px;
                        height: 12px;
                        background: #ddd;
                        border-radius: 5px;
                    }
                    .jspDrag {
                        padding: 0;
                        border-radius: 5px;
                        background: #c1c1c1;
                    }
                    .scroll_cards {
                        width: 9000px;
                        margin-left: -20px;
                        
                        & > li {
                            float: left;
                        }
                        .front {
                            width: 100%;
                            height: 100%;
                            position: relative;
                            z-index: 3;
                            padding: 30px;
                        }
                        .back {
                            width: 100%;
                            height: 100%;
                            position: absolute;
                            top: 0;
                            left: 0;
                            z-index: 2;
                            opacity: 0;
                            transform: rotateY(-180deg);
                            -webkit-transform: rotateY(-180deg);
                            -moz-transform: rotateY(-180deg);
                            padding: 30px;
                            background: rgba(0,0,0,.5);
                        }
                        .pro-desc {
                            line-height: 24px;
                            word-break: break-all;
                        }

                        .cards > li {
                            width: 300px;
                            height: 320px;
                            margin-left: 20px;                            
                            float: left;
                            color: #fff;
                            position: relative;
                            overflow: hidden;

                            &:hover {
                                .front {
                                    transform: rotateY(180deg);
                                    -webkit-transform: rotateY(180deg);
                                    -moz-transform: rotateY(180deg);
                                    opacity: 0;
                                }
                                .back {
                                    transform: rotateY(0);
                                    -webkit-transform: rotateY(0);
                                    -moz-transform: rotateY(0);
                                    z-index: 4;
                                    opacity: 1;
                                }
                            }
                        }
                        .pro_name {
                            font-size: 14px;
                            margin-bottom: 7px;
                        }
                        h3 {
                            font-size: 18px;
                            height: 60px;
                            line-height: 30px;
                            margin-bottom: 20px;
                            overflow: hidden;
                        }
                        .pro_lists {
                            li {
                                &:before{
                                    content: '·';
                                    font-size: 25px;
                                    width: 0;
                                    @extend .vam;
                                    margin-right: 10px;
                                }
                                font-size: 14px;
                                line-height: 24px;
                                width: 100%;
                                @extend .substr;
                            }
                        }
                    }
                }
            }
            .row_3 {
                padding: 60px 0;
                width: 100%;
                background: #fff;
                position: relative;
                .teacher_module {
                    padding-bottom: 50px;
                }
                .slider_content {
                    @extend .W1160;
                    position: relative;
                }
                .user_nav {
                    position: absolute;
                    left: 0;
                    bottom: -35px;
                    width: 100%;
                    z-index: 10;
                    text-align: center;
                    font-size: 0;
                    img {
                        max-width: 100%;
                    }
                    li {
                        @extend .vam;
                        width: 70px;
                        height: 70px;
                        position: relative;
                        border-radius: 50%;
                        overflow: hidden;
                        background: lightblue;
                        margin: 0 15px;
                        border: 2px solid #fff;
                        @extend .transition;
                        cursor: pointer;
                        i {
                            display: block;
                            width: 100%;
                            height: 100%;
                            position: absolute;
                            left: 0;
                            top: 0;
                            z-index: 1;
                            background: rgba(255,255,255,.4);
                            border-radius: 50%;
                        }
                    }
                    li:hover, li.active {
                        transform: scale(1.4);-ms-transform: scale(1.4);
                        -webkit-transform: scale(1.4);
                        -o-transform: scale(1.4);
                        -moz-transform: scale(1.4);
                        i {
                            display: none;
                        }
                    }
                }

                .slider_lists {
                    height: 450px;
                    overflow: hidden;
                    li:first-child {
                        display: block;
                    }
                    li {
                        position: relative;
                        height: 100%;
                        display: none;
                        .info {
                            width: 100%;
                            height: 100%;
                            background: rgba(0,0,0,.5);
                            filter: alpha(opacity=50);
                            position: absolute;
                            left: 0;
                            top: 0;
                            padding: 116px 180px 110px;
                        }
                        img {
                            width: 1160px;
                            height: 450px;
                            max-width: 100%;
                        }
                        h6, h4 {
                            color: #fff;
                            text-align: center;
                        }
                        h6 {
                            font-size: 20px;
                            margin-bottom: 7px;
                        }
                        h4 {
                            font-size: 40px;
                            margin-bottom: 29px;
                        }
                        p {
                           font-size: 18px;
                           color: #fff;
                           height: 77px;
                           overflow: hidden;
                           text-align: center;
                        }
                    } 
                }
                .student_module {
                    padding: 60px 0 75px;
                    .prev, .next {
                        position: absolute;
                        top: 50%;
                        width: 42px;
                        height: 70px;
                        margin-top: -40px;
                        background: url(../images/course_introduce/sprite.png) 0 0 no-repeat;
                    }
                    .prev {
                        left: -60px;
                        background-position: 0 -724px;
                    }
                    .next {
                        right: -60px;
                        background-position: 0 -804px;
                    }
                    .pageState {
                        display: block;
                        text-align: center;
                        font-size: 14px;
                        color: #999;
                        letter-spacing: 4px;
                        span {
                            font-size: 28px;
                            color: #666;
                        }
                    }
                }
                .works_show {
                    width: 800px;
                    margin: 0 auto;
                    position: relative;
                }
                .bx-wrapper {
                    .bx-controls-direction {
                        a {
                            width: 42px;
                            height: 70px;
                            margin-top: -40px;
                        }
                    }
                    .bx-prev {
                        @extend .sprite;
                        left: -60px;
                        background-position: 0 -724px;
                    }
                    .bx-next {
                        @extend .sprite;
                        right: -60px;
                        background-position: 0 -804px;
                    }
                    .bx-pager {
                        bottom: -50px;
                        color: #999;
                        font-size: 14px;
                        font-weight: 400;
                        i {
                            font-size: 28px;
                            color: #666;
                        }
                    }
                }
            }
            .row_4 {
                width: 100%;
                height: 600px;
                position: relative;
                .inner {
                    @extend .W1160;
                    height: 100%;
                    position: relative;
                }
                .student_desc {
                    width: 400px;
                    height: 450px;
                    background-color: rgba(255,255,255,.9);
                    padding: 46px;
                    position: absolute;
                    left: 80px;
                    top: 75px;
                    h4, .desc_con, .second, .last{
                        color: #666;
                    }
                    h4, .second {
                        font-size: 26px;
                    }
                    h4 {
                        margin-bottom: 10px;
                    }
                    .desc_con, .last {
                        font-size: 16px;
                    }
                    .desc_con {                        
                        font-style: italic;
                        height: 170px;
                        line-height: 28px;
                        overflow: hidden;
                        margin-bottom: 30px;
                    }
                    .first {
                        font-size: 12px;
                        color: #999;
                    }
                    .second {
                        margin: 5px 0;
                    }
                    .video {
                        width: 90px;
                        height: 90px;
                        position: absolute;
                        right: -45px;
                        bottom: 46px;
                        z-index: 30;
                        @extend .sprite;
                        cursor: pointer;
                        background-position: 0 -144px;
                    }
                }
                .video-warp {
                    position: absolute;
                    top: 0;
                    left: 0;
                    width: 100%;
                    height: 100%;
                    background: rgba(0,0,0,.8);
                    z-index: 40;
                    display: none;
                }
                .video-con {
                    width: 1160px;
                    height: 100%;
                    margin: 0 auto;
                    position: relative;
                }
                .close-video {
                    display: block;
                    width: 30px;
                    height: 30px;
                    border-radius: 50%;
                    position: absolute;
                    right: 10px;
                    top: 10px;
                    z-index: 10;
                    background: #cfcfcf url(../images/close2.png) center center no-repeat;
                }
            }
            .row_5 {
                padding: 40px 0 70px;
                font-size: 14px;
                .inner {
                    @extend .W1160;
                }
                h4 {
                    color: #999;
                    text-align: center;
                    margin-bottom: 30px;
                }
                .company_lists {
                    text-align: center;
                    margin-left: -60px;
                    margin-bottom: 60px;
                    &:before, &:after{
                        @extend .CLEARFIX;
                    }
                    li {
                        width: 155px;
                        height: 50px;
                        display: inline-block;
                        margin-left: 60px;
                    }
                }
                .tab_module {
                    width: 800px;
                    margin: 0 auto 70px;;
                    background-color: #fff;
                    border: 1px solid #e8e8e8;
                    box-sizing: border-box;
                }
                .tab_nav {
                    &:before,&:after{
                        @extend .CLEARFIX;
                    }
                    li {
                       width: 50%;
                       height: 56px;
                       background-color: #f5f5f5;
                       line-height: 56px;
                       color: #666;
                       float: left;
                       cursor: pointer;
                       text-align: center;
                       border-right: 1px solid #e8e8e8;
                       border-bottom: 1px solid #e8e8e8;
                    }
                    li:last-child {
                        border-right: none;
                    }
                    .active {
                        background-color: #fff;
                        border-bottom: 1px solid #fff;
                    }
                }
                .tab_con {
                    padding: 45px;
                    height: 446px;
                    & > div {
                        display: none;
                    }
                    & > div:first-child {
                        display: block;
                    }
                    .lists_1 {
                        &:before,&:after {
                            @extend .CLEARFIX;
                        }
                        li {
                            width: 50%;
                            float: left;
                            height: 48px;
                            line-height: 48px;
                            @extend .substr;
                        }
                        li:nth-child(2n){
                            padding-left: 30px;
                        }
                        li:before {
                            content: '';
                            @extend .vam;
                            width: 20px;
                            height: 20px;
                            @extend .sprite;
                            background-position: 0 -244px;
                            margin-right: 20px;
                        }
                    }
                    p {
                        line-height: 28px;
                        height: 85px;
                        overflow: hidden;
                        margin-bottom: 30px;
                        font-size: 16px;
                        color: #666;
                        a {
                            margin-left: 20px;
                            color: #00b38a;
                            font-size: 16px;
                        }
                    }
                    .lists_2 {
                        text-align: center;
                        &:before, &:after {
                            @extend .CLEARFIX;
                        }
                        li {
                            @extend .vam;
                            margin-left: 36px;
                            color: #666;
                        }
                        li:first-child {
                            margin-left: 0;
                        }
                        li:before {
                            content: '';
                            display: block;
                            width: 80px;
                            height: 80px;
                            margin: 0 auto 20px;
                            @extend .sprite;                            
                        }
                        .first:before {
                            background-position: 0 -274px;
                        }
                        .second:before {
                            background-position: 0 -364px;
                        }
                        .three:before {
                            background-position: 0 -454px;
                        }
                        .four:before {
                            background-position: 0 -544px;
                        }
                        .five:before {
                            background-position: 0 -634px;
                        }
                    }
                }
                .market_value, .maizi_price, .maizi_value {
                    text-align: center;
                }
                .market_value {
                    font-size: 18px;
                    color: #999;
                    margin-bottom: 20px;
                    span {
                        text-decoration: line-through;
                    }
                }
                .maizi_price {
                    font-size: 16px;
                    color: #666;
                }
                .maizi_value {
                    font-size: 40px;
                    line-height: 44px;
                    color: #5ecfba;
                }
            }
        }
        .course_inter {
            background: #fff;
        }
        .course_interaction {
            width: 900px;
            margin: 0 auto;
            padding: 60px 0;
            .row_1, .row_2, .row_3, .row_4 {
                h2 {
                    font-size: 24px;
                    color: #333;
                    text-align: center;
                    margin-bottom: 30px;
                }
                .user_info {
                    a, span, i {
                        @extend .vam;
                    }
                }                
                .user {
                    width: 28px;
                    height: 28px;
                    border-radius: 50%;
                    overflow: hidden;
                    margin-right: 10px;
                }
                .nike_name {
                    width: 80px;
                    color: #999;
                    font-size: 14px;
                    @extend .substr;
                    margin-right: 10px;
                }
                .from_where {
                    width: 80px;
                    height: 23px;
                    line-height: 23px;
                    border: 1px solid #ccc;
                    text-align: center;
                    margin-right: 10px;
                }
                .from_link {
                   font-size: 14px;
                    i {
                       color: #666;
                       margin-right: 10px;
                    }
                    a:before {
                       content: '·';
                       width: 0;
                       margin-right: 10px;
                       color: #666;
                    }
                    a {
                       color: #00b38a;
                    }
                }
            }
            .row_2, .row_3, .row_4 {
                margin-top: 60px;
            }
            .row_1 {
                ul {
                    & >li:first-child {
                        border-top: 1px solid #ededed;
                    }
                    & > li {
                        padding: 30px 0;
                        border-bottom: 1px solid #ededed;
                       .tag {
                            margin-bottom: 10px;
                            a {
                                font-size: 12px;
                                color: #999;
                                margin-right: 10px;
                            }
                            a:before {
                                @extend .vam;
                                content: '';
                                width: 14px;
                                height: 14px;
                                margin-right: 10px;
                                background: url(../images/article_Icon.png) 0 -160px no-repeat;
                            }
                            a:hover {
                                color: #5ecfba;
                            }
                       }
                       h3 {
                            font-size: 0;
                            margin-bottom: 20px;
                            i, a {
                                @extend .vam;
                            }
                            i {
                                font-size: 12px;
                                color: #fff;
                                width: 40px;
                                height: 22px;
                                line-height: 22px;
                                background: #ff725f;
                                border-radius: 2px;
                                text-align: center;
                                margin-right: 10px;
                            }
                            a {
                                font-size: 22px;
                                color: #333;
                                width: 850px;
                                transition: .5s;
                                @extend .substr;
                            }
                            a:hover {
                                color: #5ecfba;
                            }
                       }
                       .desc {
                            font-size: 14px;
                            color: #999;
                            max-height: 80px;
                            overflow: hidden;
                            margin-bottom: 20px;
                       }
                       .news_img {
                            margin-left: -20px;
                            margin-bottom: 20px;
                            &:before, &:after {
                                @extend .CLEARFIX;
                            }
                            li {
                                width: 210px;
                                height: 140px;
                                margin-left: 20px;
                                float: left;
                            }
                            img {
                                width: 100%;
                                height: 100%;
                                max-width: 100%;
                            }
                       }
                    }
                }
            }
            .from {
                 position: relative;
                 height: 30px;
                 .pos_l, .pos_r {
                     position: absolute;
                     top: 0;
                 }
                 .pos_l {
                     left: 0;
                     
                     img {
                         max-width: 100%;
                     }

                 }
                 .pos_r {
                     right: 0;
                     top: 8px;
                 }

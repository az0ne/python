    font-size: .6rem;
    color: #666;
    margin-left: .9rem
}

.course-chapter li:last-child, .course-chapter li:nth-child(5) {
    border: none
}

.course-chapter .course-expand {
    color: #5ecfba;
    font-size: .6rem;
    text-align: center;
    line-height: 1rem;
    margin-top: .15rem
}

.course-chapter .course-expand:after {
    width: .65rem;
    height: .425rem;
    background-position: 88.5% 25%;
    background-size: 1326.92% 970.59%;
    margin-left: .2rem
}

.course-chapter .active a {
    color: #5ecfba
}

.course-chapter .active:before {
    border-left: .45rem solid #5ecfba;
    border-top: .3rem solid transparent;
    border-bottom: .3rem solid transparent;
    float: left;
    margin-top: .5rem
}

.course-teacher {
    padding: .65rem .65rem;
    overflow: hidden
}

.course-teacher dt {
    width: 3.2rem;
    height: 3.2rem;
    overflow: hidden;
    float: left
}

.course-teacher dd {
    margin-left: 3.85rem
}

.course-teacher dd strong {
    font-size: .75rem;
    line-height: 1.1rem
}

.course-teacher dd p {
    font-size: .6rem;
    color: #666;
    line-height: .9rem;
    margin-top: .375rem;
    display: table;
    height: 1px
}

.course-exchange ul {
    padding: 0 4%;
    overflow: hidden;
    border-bottom: 2px solid #ececec
}

.course-exchange li {
    border-bottom: 2px solid #eee
}

.course-exchange li, .course-exchange .comment-next {
    padding-top: .6rem
}

.course-exchange li .comment-next {
    margin-left: 1.6rem
}

.course-exchange li > p, .course-exchange .comment-next > p {
    width: 1.45rem;
    height: 1.45rem;
    overflow: hidden;
    border-radius: 100%;
    float: left
}

.course-exchange .info {
    margin-left: 2.1rem;
    padding-bottom: .6rem;
    font-size: .6rem
}

.course-exchange li:last-child .info {
    border: 0
}

.course-exchange .info time {
    color: #999;
    font-size: .5rem;
    float: right
}

.course-exchange .info strong {
    color: #666;
    display: inline-block
}

.course-exchange .info p {
    color: #333;
    line-height: .85rem;
    margin: .2rem 0 .5rem;
    display: table;
    height: 1px;
    word-break: break-word
}

.course-exchange .loading {
    color: #5ecfba;
    font-size: .6rem;
    text-align: center;
    line-height: 1.1rem;
    margin: .4rem 0 1.6rem;
    display: table;
    width: 100%
}

.course-exchange .info .comment-next {
    border-top: 2px solid #eee;
    margin: 0
}

.course-exchange .info .info {
    padding: 0
}

.course-exchange .info .info p {
    margin-bottom: 0
}

.course-exchange .info .comment-next em {
    display: inline-block;
    line-height: .75rem;
    color: #fff;
    background: #5ecfba;
    padding: 0 .2rem;
    border-radius: 3px;
    margin-left: .35rem
}

.course-down {
    background: #f5f5f5;
    height: 3.15rem
}

.course-down a {
    position: fixed;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 2.55rem;
    line-height: 2.55rem;
    background: rgba(0, 0, 0, 0.75);
    color: #fff;
    font-size: .7rem;
    text-align: center
}

.course-down a:before {
    width: 1.75rem;
    height: 1.75rem;
    background-position: 0 0;
    margin-right: .6rem
}

.personal-page-top {
    height: 5.35rem;
    background: url(/static/images/mz_wap/presonal-page-bg.jpg) no-repeat;
    background-size: cover;

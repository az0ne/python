{% extends "mz_common/global.html" %}
{% block title %}{{ seo.seo_title }}{% endblock %}
{% block keywords %}{{ seo.seo_keyword }}{% endblock %}
{% block description %}{{ seo.seo_description }}{% endblock %}
{% block custom_css %}
<link rel="stylesheet" href="/css/userCenter.css">
<link rel="stylesheet" type="text/css" href="/2016//lib/layer/skin/layer.css">
<link rel="stylesheet" type="text/css" href="/2016/lib/Jcrop/jquery.Jcrop.min.css">
{% endblock %}
{% block csstype %}userCenter{% endblock %}
{% block container %}
    {% include 'header/index_header.html' %}
    {% include 'mz_usercenter/div_user_header.html' %}
    <div class="center personalCmain">

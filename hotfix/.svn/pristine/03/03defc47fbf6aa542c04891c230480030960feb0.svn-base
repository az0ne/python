/* 预约试学样式 */
@import 'core/_var';
.pro-answer {
    i {
        font-style: normal;
    }
}
.answer-con {
    background-color: #fff;
    .status-tag {
        @extend .vam;
        padding: 8px 10px;
        box-sizing: border-box;
        height: 30px;
        margin-left: 20px;
        text-align: center;
    }
    .no {
        border: 1px solid #ff721d;        
        color: #ff721d;        
    }
    .go-on {
        border: 1px solid #5ecfba;
        color: #5ecfba;
    }

    .end {
        color: #999;
        border: 1px solid #999;
    }

    h1 {
        padding: 40px 50px 20px;
        box-sizing: border-box;
    }
    .see-more {
        margin-bottom: 30px;
        span {
            @extend .vam;
            vertical-align: bottom;
            line-height: 20px;
        }
        .three {
            margin-left: 10px;
        }
        
        .four {
            margin-left: 30px;
        }
    }
    .see-more, .pro-info {
        padding: 0 50px;
        box-sizing: border-box;
    }
    .pro-info {
        display: none;
    }
    .show-detail {
        color: #00b38a;
        float: right;
        cursor: pointer;
        margin-top: 5px;
    }
    .tab-nav {
        padding-left: 50px;
        &:before, &:after {
            @extend .CLEARFIX;
        }
        li {
            float: left;
            width: 170px;
            height: 50px;
            line-height: 50px;
            margin-right: 10px;
            text-align: center;
            color: #666;
            background-color: #f5f5f5;
            cursor: pointer;
            border: 1px solid #f5f5f5;
            border-bottom: none;
            box-sizing: border-box;
            i {
                @extend .vam;
                width: 24px;
                height: 24px;
                line-height: 24px;
                background-color: #5ecfba;
                color: #fff;
                margin-right: 10px;
                border-radius: 50%;
                margin-top: -2px;
            }
        }
        .active {
            border: 1px solid #ddd;
            background-color: #fff;
            border-bottom: none;
            &:after {
                content: '';
                width: 100%;
                display: block;
                border-bottom: 10px solid #fff;
                position: relative;
                bottom: 3px;
            }
        }
    }
    .tab-box {
        border-top: 1px solid #ddd;
        padding: 40px 50px 70px;
        box-sizing: border-box;
        transition: .5s;
        &:before, &:after {
            @extend .CLEARFIX;
        }
        & > li {
            display: none;
            transition: .5s;
        }
        .left {
            width: 690px;
            float: left;
        }
        .right {
            width: 330px;
            float: right;
        }
        .living {
            margin-bottom: 20px;
        }
        .class-living {
            border: 1px solid #ddd;
            padding: 20px;
            box-sizing: border-box;
            margin-bottom: 20px;
            position: relative;
            .first {
                @extend .vam;
                width: 44px;
                height: 44px;
                background: url(../images/free_learn/sprite.png) 0 0 no-repeat;
                margin-right: 20px;
            }
            .second {
                @extend .vam;
                width: 290px;
                span {
                    display: block;
                }
            }
            .three {
                color: #ff721d;
                margin-right: 20px;
            }
            .button {
                @extend .vam;
                width: 100px;
                height: 40px;
                line-height: 40px;                
                color: #fff;
                font-size: 14px;
                text-align: center;
                border-radius: 2px;
                position: absolute;
                top: 25px;
                right: 20px;
            }
            .no-start {
                background: #faae0b;
            }
            .in {
                background: #5ecfba;
            }
            .out {
                background: #ccc;
                cursor: no-drop;
            }
        }
        .show-remark {
            @extend .vam;
            #display: none;
            width: 150px;
            height: 40px;
            line-height: 40px;
            text-align: center;
            border: 1px solid #5ecfba;
            box-sizing: border-box;
            border-radius: 2px;
            margin-top: 15px;
            transition: .5s;
            cursor: pointer;
            &:hover {
                color: #fff;
                background-color: #5ecfba;
            }
        }

        .remark-con {
            margin-top: 15px;
        }
        .tips {
            margin-bottom: 40px;
        }
        .base-title {
            margin-bottom: 10px;
        }
        .sub-title {
            margin-bottom: 20px;
        }
        .base-list {
            li {
                line-height: 26px;
            }
            li:before {
                @extend .vam;
                content: '·';
                font-size: 18px;
                width: 0;
                margin-right: 10px;
            }
            a {
                @extend .vam;
                font-size: 14px;
                color: #666;
            }
        }
        .quiz {
            padding: 30px;
            box-sizing: border-box;
            border: 1px solid #ddd;
            &:before, &:after {
                @extend .CLEARFIX;
            }
            img {
                width: 92px;
                height: 92px;
                max-width: 100%;
                border-radius: 50%;
            }
            .left, .right {
                @extend .vam;
            }
            .left {
                width: 92px;
                span {
                    margin-top: 15px;
                    display: block;
                    text-align: center;
                }
            }
            .right {
                width: 155px;
                position: relative;
            }
            .answer-time {
                margin-bottom: 10px;
            }
            .tiwen {
                display: block;
                width: 150px;
                height: 40px;
                line-height: 40px;
                font-size: 14px;
                color: #fff;
                border-radius: 2px;
                text-align: center;
                margin-top: 10px;
            }
            .in {
                background: #5ecfba;
            }
            .out {
                background: rgba(94,207,186,.5);
            }
            .hover-tips {
                position: absolute;
                top: inherit;
                bottom: -55px;
                left: -7px;
            }
            .hover-tips:before {
                border-color: transparent transparent #333 transparent;
                position: absolute;
                left:50%;
                margin-left: -7px;
                top: -16px;
            }
        }
        .notice-content {
            overflow: hidden;
            height: 0;
            -webkit-transition: 1s;
            -moz-transition: 1s;
            transition: 1s;
        }
        .notice-msg {
            padding: 10px 20px;
            box-sizing: border-box;
            background-color: #fff9df;            
        }
        .lesson-list {
            li {
                padding: 0 15px;
                box-sizing: border-box;               
                border-bottom: 1px solid #f5f5f5;
                font-size: 0;
                a {
                    @extend .vam;
                    width: 630px;
                    color: #666;
                    height: 42px;
                    line-height: 42px;
                    font-size: 14px;
                }
                &:hover {
                    background-color: #f5f5f5;
                }
            }
            li:first-child {
                border-top: 1px solid #f5f5f5;
            }
            .ali:not(.last):before, .finish:not(.last):before, .over:not(.last):before {
                @extend .vam;
                content: '';
                width: 20px;
                height: 20px;
                margin-right: 10px;
                background: url(../images/free_learn/sprite.png) 7px -49px no-repeat;
            }
            .finish:not(.last):before {
                background-position: 0 -304px;
            }

            .col-r {
                float: right;
                color: #999;
            }
            .test:not(.last):before {
                background-position: 6px -70px;
            }
            .over:not(.last):before {
                background-position: 0 -274px;
            }
            .select {
                background-color: #f5f5f5;
            }
            .last {
                border-bottom: none;
                i {
                    padding: 4px 8px;
                    box-sizing: border-box;
                    border-radius: 2px;
                    background-color: #5ecfba;
                    color: #fff;
                    font-size: 12px;
                    margin-right: 10px;
                }
            }
        }
        .study-data {
            margin-top: 30px;
            h2 {
                padding-bottom: 10px;
                box-sizing: border-box;
                border-bottom: 1px solid #f5f5f5;
            }
            .hover-tips {
                top: 0;
            }
        }
        .study-list {
            padding-top: 10px;
            box-sizing: border-box;
            li {
                line-height: 28px;
            }
            li:before {
                @extend .vam;
                content: '·';
                font-size: 24px;
                width: 0;
                margin-right: 15px;
            }
            a {
                @extend .vam;
                font-size: 14px;
                color: #666;
            }
        }
        .score-title {
            margin-bottom: 20px;
        }
        .pro-score {
            @extend .vam;
            vertical-align: top;
            width: 140px;
            height: 80px;
            margin-right: 20px;
            background-color: #fff9df;
            text-align: center;
            color: #666;
            font-size: 14px;
        }
        .no-task, .wait {
            line-height: 80px;            
        }
        .score {
            padding: 10px 0;
            box-sizing: border-box;
            p {
               font-size: 14px;
               color: #666; 
            }
            span {
                font-size: 30px;
                color: #1bbc9b;
            }
        }
        .teacher-remark {
            @extend .vam;
            vertical-align: top;
            width: 520px;
        }
        .answer-living {
            margin-bottom: 10px;
            margin-top: 30px;
        }
        .student-data {
            margin-top: 30px;
            margin-bottom: 10px;
            h2 {
                padding-bottom: 20px;
                display: inline-block;
            }
            .hover-tips {
                top: -5px;
                left: 20px;
            }
        }
        .student-list {
            li {
                font-size: 0;
                margin-bottom: 20px;
            }
            i, img, .info {
                @extend .vam;
                vertical-align: top;
            }
            i {
                width: 18px;
                height: 23px;
                color: #fff;
                font-size: 14px;
                text-align: center;
                background: url(../images/free_learn/sprite.png) 0 -96px no-repeat;
            }
            img {
                width: 130px;
                height: 92px;
                max-width: 100%;
                margin-left: 20px;
            }
            .info {
                width: 140px;
                margin-left: 20px;
            }
            p {
                height: 42px;
                overflow: hidden;
                margin-bottom: 20px;
            }
            .down-source {
                @extend .vam;
                width: 100px;
                height: 30px;
                font-size: 14px;
                color: #5ecfba;
                border: 1px solid #5ecfba;
                border-radius: 2px;
                padding: 5px 0;
                box-sizing: border-box;
                text-align: center;
                transition: .5s;
                &:before {
                    @extend .vam;
                    content: '';
                    width: 14px;
                    height: 12px;
                    margin-right: 10px;
                    transition: .5s;
                    background: url(../images/free_learn/sprite.png) 0 -129px no-repeat;
                }
            }
            .down-source:hover {
                background-color: #00b38a;
                color: #fff;
                &:before {
                    background: url(../images/free_learn/sprite.png) 0 -151px no-repeat;
                }
            }
        }

        // 课程推荐
        .title-outline, .title-pro, .zixun-lesson, .title-thinking {
            margin-bottom: 20px;
        }
        .outline-list {
            margin-left: -20px;
            margin-bottom: 30px;
            &:before, &:after {
                @extend .CLEARFIX;
            }
            & > li:nth-child(0) {
                background-color: #24bcac;
            }
            & > li:nth-child(1) {
                background-color: #5496d1;
            }
            & > li:nth-child(2) {
                background-color: #76c583;
            }
            & > li:nth-child(3) {
                background-color: #6951a1;
            }
            & > li:nth-child(4) {
                background-color: #f48147;
            }

            & > li {
                width: 196px;
                height: 84px;
                float: left;
                margin-left: 20px;
                padding: 20px;
                box-sizing: border-box;
                background: #5ecfba;
                position: relative;
                cursor: pointer;
                &:hover {
                    .outline-hover {
                        display: block;
                        visibility: visible;
                        opacity: 1;
                        filter: alpha(opacity=100);
                        max-width: 300px;
                        -webkit-transform: scale(1);
                        -ms-transform: scale(1);
                        transform: scale(1);
                    }
                }
            }
        }
        .outline-hover {
            width: 300px;
            min-height: 249px;
            padding: 20px;
            background-color: #fff;
            box-sizing: border-box;
            border: 1px solid #f5f5f5;
            box-shadow: 0 0 20px rgba(0, 0, 0, 0.1);
            position: absolute;
            top: -275px;
            left: -40px;
            z-index: 10;
            -webkit-transition: opacity 0.25s, transform .3s;
            -moz-transition: opacity 0.25s, transform .3s;
            transition: opacity 0.25s, transform .3s;
            opacity: 0;
            filter: alpha(opacity=0);
            max-width: 300px;
            -webkit-transform: scale(0.01);
            -ms-transform: scale(0.01);
            transform: scale(0.01);
            -webkit-transform-origin: center bottom;
            -ms-transform-origin: center bottom;
            transform-origin: center bottom;
            &:after {
                content: '';
                width: 0;
                height: 0;
                position: absolute;
                bottom: -14px;
                left: 50%;
                margin-left: -25px;
                display: inline-block;
                padding: 14px;
                background: #ffffff;
                box-shadow: -2px 0 0 #fff,-2px 0 0 #fff,0 2px 5px rgba(0,0,0,.1),2px 2px 5px rgba(0,0,0,0.1);
                transform:rotate(45deg);
                -ms-transform:rotate(45deg);
                -moz-transform:rotate(45deg);
                -webkit-transform:rotate(45deg);
                -o-transform:rotate(45deg);
            }
        }
        .task-dot {
            margin-bottom: 20px;
        }
        .task-list {
            margin-bottom: 40px;
            li {
                line-height: 24px;
                font-size: 14px;
                color: #666;
            }
            li:before {
                @extend .vam;
                content: '·';
                font-size: 24px;
                width: 0;
                margin-right: 15px;
            }
        }


        .pro-list {
            margin-left: -35px;
            &:before, &:after {
                @extend .CLEARFIX;
            }
            li {
                width: 330px;
                height: 138px;
                float: left;
                margin-left: 35px;
            }
            img {
                width: 100%;
                height: 100%;
                max-width: 100%;
            }
        }
        .zixun-lesson {
            display: block;
            width: 220px;
            height: 54px;
            line-height: 54px;
            border: 1px solid #5ecfba;
            box-sizing: border-box;
            border-radius: 2px;
            text-align: center;
            transition: .5s;
            margin: 30px auto;
            &:hover {
                color: #fff;
                background-color: #5ecfba;
            }
        }
        .thinking-list {
            margin-left: -35px;
            margin-bottom: 60px;
            &:before, &:after {
                @extend .CLEARFIX;
            }
            li {
                width: 330px;
                height: 266px;
                float: left;
                margin-left: 35px;
                background-color: #5ecfba;
            }
            img {
                width: 100%;
                height: 100%;
                max-width: 100%;
            }
        }
        .zixun {
            text-align: center;
            p {
                line-height: 40px;
            }
            .second {
                margin-bottom: 30px;
            }
            .four {
                span {
                    text-decoration: line-through;
                }
            }
        }
        .now-zixun {
            display: block;
            width: 180px;
            height: 52px;
            line-height: 52px;
            color: #fff;
            font-size: 16px;
            background-color: #5ecfba;
            border-radius: 2px;
            margin: 25px auto 0;
            transition: .5s;
        }
    }
}
// 满意度调查样式
#satisfy-examen {
    .modal-content {
        padding: 30px;
    }
    .modal-head {
        width: 100%;
        padding: 20px;
        background-color: #fff9df;
        box-sizing: border-box;
        margin-bottom: 30px;
        p {
            line-height: 24px;
        }
    }
    .satisfy-list {
        li {
            padding-bottom: 10px;
            box-sizing: border-box;
            border-bottom: 1px solid #e8e8e8;
            margin-top: 20px;
        }
        .first {
            margin-bottom: 10px;
        }
        .last {
            height: 40px;
            padding: 10px 20px;
            box-sizing: border-box;
        }
        label {
            @extend .vam;
            font-weight: 400;
        }
        label:before {
            @extend .vam;
            content: '';
            width: 14px;
            height: 14px;
            margin-right: 10px;
            margin-top: -2px;
            cursor: pointer;
            background: url(../images/free_learn/sprite.png) 0 -226px no-repeat;
        }
        label.select:before {
            background: url(../images/free_learn/sprite.png) 0 -250px no-repeat;
        }
        .now {
            .last {
                background-color: #f5f5f5;
            }
        }
    }
    .button-group {
        margin-top: 30px;
        text-align: center;
    }
    .submit, .next-time {
        @extend .vam;
        width: 200px;
        height: 40px;
        line-height: 40px;
        border-radius: 2px;
        font-size: 14px;
        box-sizing: border-box;
    }
    .submit {
        color: #fff;
        background-color: #5ecfba;
    }
    .next-time {
        margin-left: 40px;
        color: #5ecfba;
        border: 1px solid #5ecfba;
        transition: .3s;
        &:hover {
            color: #fff;
            background-color: #5ecfba;
        }
    }

}
.footer-section {
    width: 100%;
    height: 70px;
    background: #fff;
    z-index: 1000;
    .inner {
        @extend .W1160;
        padding: 15px 0;
        box-sizing: border-box;
    }
    .min-3000 {
        color: #ff721d;
    }
    .col_l {
        float: left;
    }
    .col_r {
        float: right;
        span, .enroll {
           @extend .vam; 
        }
    }
    .enroll {        
        width: 120px;
        height: 40px;
        line-height: 40px;
        text-align: center;
        color: #fff;
        background: #5ecfba;
        margin-left: 20px;
        border-radius: 2px;
        font-size: 14px;
    }
}
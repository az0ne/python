@mixin borderradius($px){
  -webkit-border-radius: $px;
  -moz-border-radius: $px;
  border-radius: $px;
}
$resumebg:url(/images/resume/resumeIcon.png) no-repeat;
.resumeTip{
  padding-top: 130px;
  text-align: center;
  &>p{
    height: 257px;
  }
  strong{
    font-size: 18px;
    color: #333;
    font-weight: normal;
    padding: 40px 0 30px;
    display: block;
  }
}
.resumeA{
  width: 200px;
  height: 60px;
  line-height: 60px;
  margin: 0 auto 168px;
  display: block;
  text-align: center;
  color: #fff;
  background: #5ecfba;
  @include borderradius(5px);
  font-size: 18px;
  &:hover{
    color: #fff;
    background: #49d2ba;
  }
}
.resume{
  padding: 0;
  width: 940px;
  overflow: hidden;
  .tabTit{
    height: 70px;
    border-bottom: 1px solid #cdd1d6;
    padding: 2px 0;
    &>a{
      @extend .resumeA;
      width: 158px;
      height: 44px;
      line-height: 44px;
      margin: 15px 40px 0 0;
      float: right;
    }
    ul{
      overflow: hidden;
    }
    li{
      width: 152px;
      float: left;
      line-height: 70px;
      text-align: center;
      font-size: 18px;
      color: #b3b3b3;
      cursor: pointer;
      &.cur{
        color: #5ecfba;
        border-bottom: 3px solid #5ecfba;
      }
      em{
        font-size: 16px;
        font-style: normal;
        line-height: normal;
        color: #ff6565;
        margin-left: 5px;
      }
    }
  }
  .tabBox{
    overflow: hidden;
    padding: 0 40px;
    &>div{
      visibility: hidden;
      display: none;
      &.cur{
        visibility: visible;
        display: block;
      }
    }
    select.form-control{
      display: inline-block;
      width: auto;
      margin-right: 18px;
      height: 42px;
      font-size: 16px;
      padding:0 20px 0 26px;
      background: #f4f3f3;
      color: #888;
      option{
        background: #fff;
      }
      &:focus{
        background: #f1fbf9;
        color: #333;
        option{
          background: #fff;
        }
      }
    }
    .info{
      padding: 36px 0 200px;
      form{
        position: relative;
      }
      .control-label{
        width: 110px;
        float: left;
        text-align: right;
        font-weight: normal;
        color: #b2b2b2;
        font-size: 16px;
        padding: 0;
      }
      .form-group{
        margin-top: 24px;
        line-height: 44px;
        font-size: 18px;
        color: #333;
      }
      .form-group-right{
        margin-left: 146px;
        em.error{
          font-size: 16px;
          color: #ff6565;
          display: block;
          font-style: normal;
          height: 36px;
          line-height: 36px;
        }
      }
      .username{
        border-bottom: 1px solid #d7d7d7;
        padding: 0 10px;
        width: 300px;
        &:focus{
          border-bottom: 2px solid #5ecfba;
        }
        &.err{
          border-bottom: 2px solid #ff6565;
        }
      }
      .radio-inline{
        padding: 0;
        label{
          position: relative;
          font-weight: normal;
          padding-left: 35px;
        }
        label:before{
          width: 16px;
          height: 16px;
          background: url(../images/personal.png) 0 -510px no-repeat;
          content: "";
          display: block;
          position: absolute;
          left: 0;
          z-index: 9;
          top: 14px;
          cursor: pointer;
        }
        &.active label:before{
          background-position: -16px -510px;
        }
        input{
          width: 16px;
          height: 16px;
          margin: 14px 0 0 -35px;
          visibility: hidden;
        }
      }
      .submitBtn input{
        @extend .resumeA;
        width: 158px;
        height: 44px;
        line-height: normal;
        margin: 76px 0 0 136px;
      }
      .tips{
        font-style: normal;
        position: absolute;
        bottom: 0;
        left: 340px;
        line-height: 44px;
        font-size: 16px;
        display: none;
        &.err{
          color: #ff6565;
        }
        &.succ{
          color: #5ecfba;
        }
      }
    }
    .exper,.educ{
      padding: 20px 0 76px;
      overflow: hidden;
      .experList{
        font-size: 18px;
        li{margin-bottom: 20px;}
        .boxTop{
          height: 50px;
          line-height: 50px;
          background: #5ecfba;
          color: #fff;
          @include borderradius(5px 5px 0 0);
          .title{
            margin-left: 20px;
            float: left;
            max-width: 640px;
            overflow: hidden;
            text-overflow: ellipsis;
            white-space: nowrap;
          }
          .handle{
            float: right;
            em{
              width: 25px;
              height: 26px;
              display: inline-block;
              vertical-align: middle;
              background: $resumebg;
              font-size: 0;
              margin-right: 30px;
              cursor: pointer;
            }
            .del{
              background-position: 0 0;
            }
            .edit{
              background-position: 0 -26px;
            }
          }
        }
        .boxCon{
          border: 1px solid #d7d7d7;
          border-top: none;
          @include borderradius(0 0 5px 5px);
          padding: 5px 20px 22px;
          p{
            line-height: 30px;
            margin-top: 10px;
            display: table;
          }
          strong{
            font-weight: normal;
            width: 92px;
            color: #333;
            display:table-cell;
          }
          span{
            color: #808080;
            display:table-cell;
          }
        }
      }
      .create{
        border: 1px solid #d7d7d7;
        @include borderradius(5px);
        padding: 24px 40px 20px;
        .form-group{
          margin-top: 10px;
          display: table;
        }
        .control-label{
          width: 90px;
          text-align: right;
          display: table-cell;
          vertical-align: top;
          font-weight: normal;
          line-height: 44px;
          font-size: 18px;
          color: #333;
        }
        .form-right{
          display: table-cell;
          width: 542px;
          vertical-align: top;
          &.selectTime{
            color: #888;
          }
        }
        .form-control{
          height: 44px;
          padding: 6px 12px;
        }
        .hiddenSelect{display: none;}
        select.form-control{
          background: #fff;
          margin-right: 10px;
          padding: 6px 19px;
          &:focus{
            background: #f1fbf9;
            color: #333;
            option{
              background: #fff;
            }
          }
        }
        .starttime{margin-right: 5px;}
        .endtime{margin-left: 15px;}
        .nowtime{
          margin-left: 2px;color: #333;display: inline-block;font-size: 18px;cursor: pointer;
          &:before{
            width: 16px;
            height: 16px;
            background: url(../images/personal.png) 0 -510px no-repeat;
            content: "";
            display: inline-block;
            vertical-align: middle;
            margin: -3px 10px 0 0;
          }
          &.active:before{
            background-position: -16px -510px;
          }
        }
        .jobDesc{
          .control-label{
            vertical-align: top;
          }
          .form-control{
            display: table-cell;
            vertical-align: top;
            width: 542px;
            height: 190px;
            font-size: 18px;
          }
        }
        .btns{
          margin: 16px 0 0 90px;
          span{
            width: 154px;
            height: 40px;
            line-height: 40px;
            border: 2px solid #5ecfba;
            @include borderradius(5px);
            text-align: center;
            display: inline-block;
            font-size: 18px;
            cursor: pointer;
          }
          .keep{
            color: #fff;
            margin-right: 40px;
            background: #5ecfba;
          }
          .cancel{
            color: #5ecfba;
          }
        }
        em.error{
          font-size: 16px;
          color: #ff6565;
          display: block;
          font-style: normal;
          height: 24px;
          line-height: 24px;
          margin-top: 5px;
        }
      }
      .add{
        margin-top: 40px;
        height: 42px;
        line-height: 42px;
        background: #f1fbf9;
        border: 1px dashed #5ecfba;
        text-align: center;
        font-size: 18px;
        color: #5ecfba;
        cursor: pointer;
      }
    }
    .educ{
      .experList{
        .boxCon{
          padding: 25px 20px;
          overflow: hidden;
          &>span{
            color: #808080;
            padding-right: 78px;
            display: inline;
          }
          em{
            color: #333;
            font-style: normal;
          }
        }
      }
      .create .btns{margin-top: 38px;}
    }
  }

  //简历预览
  .previewTit{
    h3{
      line-height: 74px;
      font-weight: normal;
      font-size: 18px;
      color: #5ecfba;
      margin-left: 40px;
    }
  }
  .previewList{
    padding: 30px 40px 70px;
    li{
      margin-top: 10px;
      overflow: hidden;
      font-size: 18px;
      h4{
        height: 50px;
        line-height: 50px;
        font-weight: normal;
        background: #f5f5f5;
        color: #808080;
        border-left: 4px solid #5ecfba;
        padding-left: 16px;
      }
      .con{
        color: #333;
        padding: 25px 20px;
        line-height: 32px;
        strong{
          font-weight: normal;
        }
        em{
          font-style: normal;
        }
        span{
          color: #808080;
        }
        p{margin-top: 12px;}
      }
      .infos{
        strong{
          margin-right: 18px;
        }
        em{
          margin: 0 5px;
        }
      }
      .workexper{
        p{display: table;}
        em{width: 90px;}
        em,span{display: table-cell;}
      }
      .edu{
        em{color: #333;}
        span{margin-right:76px;}
      }
    }
  }
}

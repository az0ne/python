/*****************************进度条**************************************/

.progress-box{
	width: 480px;
	height:20px;
	border-radius: 10px;
	margin: 20px 0;
	background-color: #d8d8d8;
	overflow: hidden;
	position: relative;
}
.progress1{
	width:24px;
	height: 20px;
	border-radius: 8px;
	background-color: #10C46E ;
}

.watch-video-pro{
	width: 100%;
	font-size: 12px;
	text-align: center;
	position: absolute;
	top: 0;
}
/************************** 得分记录 ******************************/

.score-record-content{
	width: 100%;
	box-sizing: border-box;
	background-color: #fff;
	padding: 20px;
}
.score-record{
	width: 72px;
	height: 25px;
	font-size: 18px;
	color: #FF6500;
}
.score-record-time{
	line-height: 40px;
	font-size: 16px;
}
.score-record-list{
	width: 1160px;
	font-size: 14px;
	margin: 0 auto 20px;
	box-sizing: border-box;
}
.score-record-list-1{
	height: 55px;
	width: 100%;
	line-height: 55px;
	border-bottom: 1px solid #E8E8E8;
}
.score-record-add{
	margin-right: 12px;
}
.score-record-prompt{
	
	color: #FF6500;
}
.score-record-list-add{
	font-size: 16px;
	line-height: 55px;
}

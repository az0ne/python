{% if not user.is_authenticated %}
<!--弹出层区块-->
<!--登录-->
<div class="modal fade" id="loginModal" tabindex="-1" role="dialog" aria-labelledby="mySmallModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-sm">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                <h4 class="modal-title" id="loginModalLabel" style="font-size: 18px;">{% if request.GET.partner and request.GET.openid %}绑定已有账号{% else %}登录{% endif %}</h4>
            </div>
            <div class="modal-body">
                <section class="box-login v5-input-txt" id="box-login">
                    <form id="login_form" action="{% url 'user:login' %}" method="post" autocomplete="off">
                        {% csrf_token %}
                        <ul>
                            <li class="form-group">{{ login_form.account_l }}</li>
                            <li class="form-group">{{ login_form.password_l }}</li>
                            {% if request.GET.partner and request.GET.openid %}
                            <input type="hidden" name="partner" value="{{request.GET.partner}}">
                            <input type="hidden" name="openid" value="{{request.GET.openid}}">
                            {% endif %}
                        </ul>
                    </form>
                    <p class="good-tips"><a href="#" data-toggle="modal" data-target="#forgetpswModal" id="btnForgetpsw" class="fr">忘记密码？</a>还没有账号？<a  phptag="phptag_register" href="" data-toggle="modal" data-target="#registerModal" id="btnRegister">{% if request.GET.partner and request.GET.openid %}立即绑定新账号{% else %}立即注册{% endif %}</a></p>
                    <div>
                        <button id="login_btn" type="button" class="btn btn-micv5 btn-block" onclick="login_form_submit('login-form-tips')">{% if request.GET.partner and request.GET.openid %}立即绑定{% else %}登录{% endif %}</button>
                    </div>
                    <div id="login-form-tips" class="tips-error bg-danger">错误提示</div>
                    {% if request.GET.partner and request.GET.openid %}

                    {% else %}
                    <div class="threeLogin"><span>其他方式登录</span><a class="nqq" href="{% url 'user:connect' %}?partner=qq"></a><a class="nwx" href="{% url 'user:connect' %}?partner=wechat"></a><!--<a class="nwb"></a>--></div>
                    {% endif %}
                </section>
            </div>
        </div>
    </div>
</div>
<!--找回密码-->
<div class="modal fade" id="forgetpswModal" tabindex="-1" role="dialog" aria-labelledby="mySmallModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-sm">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" data-toggle="modal" data-target="#loginModal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                <h4 class="modal-title" id="forgetpswModalLabel" style="font-size: 18px;">找回密码</h4>
            </div>
            <div class="modal-body">
                <section class="box-forgetpsw v5-input-txt" id="box-forgetpsw">
                    <form id="find_password_form" action="{% url 'user:find_password' %}" method="post" autocomplete="off">
                        {% csrf_token %}
                        <ul>
                            <li class="form-group">{{ find_password_form.account }}</li>
                            <li class="form-group">{{ find_password_form.captcha_f }}<span class="v5-yzm fr"><a id='findpassword-captcha-refresh' class="captcha-refresh" href="#">换一张</a></span></li>
                            <li class="form-group">&nbsp;</li>
                        </ul>
                        <div>
                            <button id="findpassword_btn" type="button" class="btn btn-micv5 btn-block" onclick="find_password_form_submit()">提交</button>
                        </div>
                        <div id="findpassword-tips" class="tips-error bg-danger">错误提示</div>
                    </form>
                </section>
            </div>
        </div>
    </div>
</div>

<!-- 忘记密码，手机验证码输入界面 -->
<div class="modal fade" id="forgetpswMobileModal" tabindex="-1" role="dialog" aria-labelledby="mySmallModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-sm">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                <h4 class="modal-title" id="forgetpswMobileModalLabel" style="font-size: 18px;">手机验证</h4>
            </div>
            <div class="modal-body">
                <section class="box-forgetpsw v5-input-txt" id="box-forgetpsw-mobile-code">
                    <form id="mobile_code_password_form" action="{% url 'user:find_password_mobile_code' %}" method="post" autocomplete="off">
                        {% csrf_token %}
                        <ul>
                            <li class="form-group" id="mobile_code_password_form_message">手机短信验证码已发送，请查收！</li>
                            <li class="form-group">{{ find_password_mobile_form.mobile_f }}</li>
                            <li class="form-group">{{ find_password_mobile_form.mobile_code_f }}</li>
                        </ul>
                        <div>
                            <button id="mobile_code_password_btn" type="button" class="btn btn-micv5 btn-block" onclick="mobile_code_password_form_submit()">下一步</button>
                        </div>
                        <div id="mobile_code_password-tips" class="tips-error bg-danger">错误提示</div>
                    </form>
                </section>
            </div>
        </div>
    </div>
</div>

<!--注册-->
<div class="modal fade" id="registerModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-md">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                <h4 class="modal-title" id="registerModalLabel" style="font-size: 18px;">{% if request.GET.partner and request.GET.openid %}绑定新账号{% else %}注册{% endif %}</h4>
            </div>
            <div class="modal-body">
                <section class="box-register v5-input-txt" id="box-register">
                    <ul class="register-tabs" role="tablist">
                        <li class="active"><a href="#register-email" role="tab" data-toggle="tab" onclick="change_form('email_register_form')">邮箱注册</a></li>
                        <li><a href="#register-mobile" role="tab" data-toggle="tab" onclick="change_form('mobile_register_form')">手机注册</a></li>
                    </ul>
                    <div class="tab-content">
                        <div class="tab-pane active" id="register-email">
                            <form id="email_register_form" action="{% url 'user:email_register' %}" method="post" autocomplete="off">
                                {% csrf_token %}
                                <div class="form-group cf_g">{{email_register_form.email}}</div>
                                <div class="form-group cf_g">{{email_register_form.password}}</div>
                                <div class="form-group cf cf_g">{{email_register_form.captcha}}<span class="v5-yzm fr"><a id='email-captcha-refresh' class="captcha-refresh" href="#">换一张</a></span></div>
                                {% if request.GET.partner and request.GET.openid %}
                                <input type="hidden" name="partner" value="{{request.GET.partner}}">
                                <input type="hidden" name="openid" value="{{request.GET.openid}}">
                                {% endif %}
                            </form>
                        </div>
                        <div class="tab-pane" id="register-mobile">
                            <form id="mobile_register_form" action="{% url 'user:mobile_register' %}" method="post" autocomplete="off">
                                {% csrf_token %}
                                <div class="form-group cf">{{mobile_register_form.captcha_m}}<span class="v5-yzm fr"><a id='mobile-captcha-refresh' class="captcha-refresh" href="#">换一张</a></span></div>
                                <div class="form-group cf">{{ mobile_register_form.mobile }}<span class="send-code fr"><button id="send_sms_btn" type="button" class="btn btn-micv5 btn-send" onclick="send_sms_code('mobile_register_form','register-tips')">发送验证码</button></span></div>
                                <div class="form-group">{{ mobile_register_form.mobile_code }}</div>
                                <div class="form-group">{{ mobile_register_form.password_m }}</div>
                                {% if request.GET.partner and request.GET.openid %}
                                <input type="hidden" name="partner" value="{{request.GET.partner}}">
                                <input type="hidden" name="openid" value="{{request.GET.openid}}">
                                {% endif %}
                            </form>
                        </div>
                    </div>
                    <p class="good-tips">已经有账号？<a href="#" data-toggle="modal" data-target="#loginModal" id="btnLogin">{% if request.GET.partner and request.GET.openid %}立即绑定已有账号{% else %}立即登录{% endif %}</a></p>
                    <div>
                        <button id="register_btn"  phptag="phptag_register_login" type="button" class="btn btn-micv5 btn-block" onclick="register_form_submit()">{% if request.GET.partner and request.GET.openid %}立即绑定{% else %}注册并登录{% endif %}</button>
                    </div>
                    <div id="register-tips" class="tips-error bg-danger">错误提示</div>
                </section>
            </div>
        </div>
    </div>
</div>
<!--zhouyi:7-30-->

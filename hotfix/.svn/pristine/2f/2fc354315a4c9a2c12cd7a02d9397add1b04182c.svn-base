$(function () {
    var documentHeight = $(".wikiArticleNavBar .wikiPageNavBar").height() + $(".head-container").height() + 20;
    $(window).scroll(function(){
        if($(window).scrollTop()>documentHeight){
            $(".wikiAdsense").css({"position":"fixed","top":"0"});
        }else{
             $(".wikiAdsense").css({"position":"relative","top":"0"});
        }
    });

});

@import 'core/reset';
@import 'core/common';
@import 'core/var';
@import 'core/function';
@import 'core/mixin';
@import 'header';

$gou: url('data:image/png;base64,') 0 0 no-repeat;
$rar: url("data:image/png;base64,");
$zip: url("data:image/png;base64,");
$text: url('data:image/png;base64,') 0 0 no-repeat;
$tea: url('data:image/png;base64,') 0 0 no-repeat;
$vip: url('data:image/png;base64,') 0 0 no-repeat;
$view: url("data:image/png;base64,");
$source: url("data:image/png;base64,");
$pulling: url("data:image/png;base64,");
$add-file: url("data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABAAAAAQCAYAAAAf8/9hAAAAAXNSR0IArs4c6QAAAARnQU1BAACxjwv8YQUAAAAJcEhZcwAADsMAAA7DAcdvqGQAAABGSURBVDhPY2DAA/r6+o719/cfwKcGrxxQ838QHjWA1mEAjKqjsNAmgz7OANR0hAyN4OgFpZPRdEAgkQ98XgClESDej8+hAMDknmpVQw3iAAAAAElFTkSuQmCC");
$remove: url("data:image/png;base64,");
$close_btn: url('data:image/png;base64,') 0 0 no-repeat;
$close_btn_hover: url('data:image/png;base64,') 0 0 no-repeat;
// 编辑器样式
$u-icon: url("data:image/png;base64,");
$h-icon: url("data:image/png;base64,");


body{
    background: #f5f5f5;
}
%ueditor {
	.bdeditor {
		height: 240px;
		background-color: #fff;
		position: relative;
		padding: 20px;
		h2 {
			line-height: 60px;
			margin-top: -20px;
		}
		.ctrl-v {
			position: absolute;
			left: 60px;
			bottom: 28px;
			color: #7a7a7a;
		}
	}
	#edui1_toolbarbox {
		position: absolute;
		@include borderradius(0);
		@include box-shadow(none);
		bottom: -70px;
		margin-left: -10px;
	}
	#edui1_toolbarboxouter {
		border: none;
		background: none;
		@include box-shadow(none);
	}
	.edui-default {
		.edui-editor {
			border: none;
		}
		.edui-toolbar {
			.edui-button {
				.edui-icon {
					width: 34px !important;
					height: 34px !important;
					background-image: $u-icon;
					background-position: center;
					background-repeat: no-repeat;
				}
				.edui-state-hover {
					.edui-button-wrap {
						background-color: transparent;
						border: none;
						padding: 1px;
					}
					.edui-icon {
						background-image: $h-icon;
						background-position: center;
						background-repeat: no-repeat;
					}
				}
			}
		}
		.edui-editor-iframeholder {
			height: 140px !important;
			overflow: hidden !important;
		}
		.edui-editor-bottomContainer {
			display: none;
		}
	}
}
.container_main{
	margin: 40px auto 0;
	width: 1160px;
	.left,.right{
		display: inline-block;
		vertical-align: top;
		background: #fff;
		h2{
			color: #15d5ab;
			font-size: 18px;
			line-height: 60px;
			font-weight: bold;
			border-bottom: 1px solid #dfe1e3;
			padding-left: 20px;
		}
	}
	.left{
		width: 870px;
		margin-right: 6px;
	}
	.project-view {
		.left, .right {
			display: table-cell;
			vertical-align: top;
		}
	}
.project-con {
	border-bottom: 1px solid #dfe1e3;
	h3 {
		line-height: 60px;
	}
	.item{
		border-bottom: 1px solid #dfe1e3;
		&:last-child{
			border-bottom: 0;
		}
	}
	.left {
		width: 120px;
		background-color: #ededed;
		padding: 30px 35px 94px;
		a {
			display: block;
			width: 48px;
			height: 48px;
			margin: 0 auto;
			position: relative;
		}
		img {
			@include borderradius(50%);
			overflow: hidden;
		}
		p {
			width: 100px;
			margin: 0 -20px;
			@extend .substr;
			color: #5f6967;
		}
		i {
			position: absolute;
			right: -4px;
			bottom: -4px;
			width: 18px;
			height: 18px;
		}
	}
	.teacher {
		background: $tea center no-repeat;
	}
	.vip {
		background: $vip center no-repeat;
	}
	.right {
		font-size: 16px;
		width: 750px;
		max-width: 740px;
		padding: 30px;
		overflow: hidden;
		word-wrap: break-word;
		position: relative;
		p,div,section{
			line-height: 28px;
			max-width: 680px;
			overflow: hidden;
			word-wrap: break-word;
		}
	}
	p {
		line-height: 28px;
	}
	
}
.return-time {
	position: absolute;
	left: 30px;
	bottom: 30px;
}
.image-view {
	@extend .vam;
	position: relative;
	&:hover {
		a {
			display: block;
		}
	}
	a {
		position: absolute;
		display: none;
		width: 160px;
		height: 40px;
		line-height: 40px;
		right: 20px;
		bottom: 20px;
		background-color: rgba(0,0,0,0.4);
		@include borderradius(6px);
		&:before {
			content: '';
			width: 24px;
			height: 24px;
			background: $view center no-repeat;
			@extend .vam;
			margin-right: 10px;
		}
	}
}
.source-lists {
	font-size: 0;
	dt, dd, .dt, .dd {
		@extend .vam;
		@extend .font16;
		padding: 10px 10px 10px 0;
		width: 50%;
	}
	dt, .dt {
		font-weight: normal;
		@extend .substr;
		&:before {
			content: '';
			height: 32px;
			margin-right: 20px;
			width: 27px;
			@extend .vam;
		}
	}
	.zip:before {
		background: #fff $zip center no-repeat;
	}
	.rar:before {
		background: #fff $rar center no-repeat;
	}
	.source, .doc, .docx, .ppt, .pptx, .xls, .pdf, .txt, .xlsx {
		&:before {
			background: #fff $source center no-repeat;
		}
	}
	.dd {
		padding-left: 10px;
		position: relative;
	}
	a {
		color: #666;
		&:hover {
			color: #15d5ab;
		}
	}
}
.return-view {
	padding: 30px 30px 160px 30px;
}
.return-con {
	padding: 20px;
	background-color: #f3f3f3;
	border: 1px solid #dfe1e3;
	position: relative;
	@extend %ueditor;
}
.project-score {
	position: absolute;
	left: 0;
	right: 0;
	bottom: 30px;
	margin: auto;
	padding: 0 30px;
}
.add-file {
	position: relative;
	display: block;
	background: #fff;
	height: 50px;
	line-height: 50px;
	overflow: hidden;
	&:before {
		content: '';
		width: 16px;
		height: 16px;
		@extend .vam;
		margin-top: -2px;
		margin-right: 10px;
		background: $add-file center no-repeat;
	}
	input {
		position: absolute;
		font-size: 100px;
		right: 0;
		top: 0;
		opacity: 0;
	}
	&:hover {
		background-color: #e0e3ea;
	}
}
.file-con {
	background-color: #fff;
	padding-left: 20px;
	padding-right: 10px;
	.progressbar {
		font-size: 0;
	}
	.line, .percent {
		@extend .vam;
	}
	.line {
		width: 240px;
		height: 20px;
		background-color: #e1e1e1;
	}
	.progress-line {
		display: block;
		height: 100%;
		background-color: #15d5ab;
	}
	.percent {
		margin-left: 20px;
	}
	.remove {
		position: absolute;
		right: 0;
		top: 0;
		bottom: 0;
		margin: auto;
		width: 24px;
		height: 24px;
		background: $remove center no-repeat;
	}
	.success, .fail {
		display: none;
		position: absolute;
		right: 40px;
		top: 0;
		bottom: 0;
		margin: auto;
	}
	.reset {
		color: #ff6a6a;
		text-decoration: underline;
		&:hover {
			color: #ff6a6a;
		}
	}
}
.project-submit {
	width: 120px;
	height: 40px;
	@include borderradius(4px);
	border: none;
	background-color: #15d5ab;
	outline: none;
	position: absolute;
	right: 0;
	bottom: -60px;
}
	.right{
		width: 280px;
		position: relative;
		.grade{
			text-align: center;
			padding: 30px 0 28px;
			img{
				height: 210px;
				width: 206px;
				margin-bottom: 20px;
			}
			.comment{
				display: none;
				font-size: 	16px;
				color: #666;
				margin: 0 44px 30px;
			}
			._btn{
				display: inline-block;
				color: #fff;
				height: 40px;
				width: 187px;
				line-height: 40px;
				border-radius: 4px;
				font-size: 16px;
				cursor: pointer;
			}
			.grade_btn{
				background: #15d5ab;
				&:hover{
					background: #28e2b9;
				}
			}
			.change_btn{
				display: none;
				background: #f86262;
				&:hover{
					background: #fa7474;
				}
			}
			.disabled_btn{
				display: none;
				cursor: auto;
				background: #d7dce9;
			}
			.on{
				background: #09b58f;
			}
			.xg_on{
				background: #e45151;
			}
		}
		.grade_float{
			position: absolute;
			bottom: -208px;
			right: 0;
			width: 600px;
			border-radius: 4px;
			background: #fff;
			text-align: center;
			padding: 22px 20px 20px;
			@include box-shadow(0 0 21px 0 rgba(0,0,0,0.11));
			&:before{
				content: '';
				position: absolute;
				right: 123px;
				top: -24px;
				border: 14px solid transparent;
				border-bottom: 14px solid #fff;
			}
			.grade_level{
				margin-bottom: 20px;
				li{
					display: inline-block;
					font-size: 0;
					img{
						width: 74px;
						height: 76px;
						margin: 5px 16px;
						cursor: pointer;
						@include transition(0.1s);
						&:hover{
							width: 84px;
							height: 86px;
							margin: 0 11px;
						}
					}
				}
				.selected{
					position: relative;
					img{
						width: 84px;
						height: 86px;
						margin: 0 11px;
					}
					&:before{
						content: '';
						position: absolute;
						top: -10px;
						right: 10px;
						display: inline-block;
						height: 20px;
						width: 20px;
						background: $gou;
					}
				}
			}
			.info{
				font-size: 16px;
				color: #666;
				margin-bottom: 12px;
			}
			.sure_grade_btn{
				display: inline-block;
				color: #fff;
				background: #15d5ab;
				font-size: 16px;
				width: 140px;
				line-height: 40px;
				border-radius: 4px;
				cursor: pointer;
				&:hover{
					background: #28e2b9;
				}
			}
			._close{
				display: inline-block;
				position: absolute;
			    right: 8px;
    			top: 8px;
				height: 14px;
				width: 14px;
				background: $close_btn;
				&:hover{
					background: $close_btn_hover;
				}
			}
		}
		._hide{
			@include opacity(0);
			@include transform(translateY(-620px));
			@include transition(transform 1s ease-in,opacity 0.3s ease-in 0.1s);
			
		}
		._show{
			@include opacity(1);
			@include transform(translateY(0));
			@include transition(transform .7s ease,opacity 0.5s ease 0.3s);
		}
	}
}
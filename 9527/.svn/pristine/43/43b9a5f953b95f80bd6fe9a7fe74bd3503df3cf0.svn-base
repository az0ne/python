.b_top, .b_top .b_promoteText {
    color: #444;
}

.bm_box {
    border: 1px solid #cdcdcd;
    padding: 5px 10px;
    margin: 0px 0 100px 5%;
    width: 382px;
}

#rcCalB {
    direction: ltr;
}

.rcExpC {
    height: 19px;
    display: block;
    padding-bottom: 5px;
    overflow: hidden;
}

#rcEqB {
    cursor: pointer;
    float: right;
    text-align: right;
}

.rcResC.b_focusTextMedium {
    /*height: 58px;*/
    margin-bottom: 3px;
}

.b_focusTextMedium {
    font: 32px/1.2em 'Microsoft YaHei',Arial,Helvetica,sans-serif;
}

.b_focusTextMedium {
    font-size: 35px;
}

.rcResI {
    /*height: 90%;*/
}

.rcABPfocus {
    border: 1px solid #0072c5;
    background-color: #fff;
    padding-top: 3px;
}

.rcABP {
    overflow: hidden;
    /*padding-right: 10px;*/
    padding-right: 2px;
    padding-left: 5px;
    padding-bottom: 5px;
    height: 100%;
    color: #111;
    border-right: 1px solid #bfdcf0;
    border-bottom: 1px solid #bfdcf0;
    border-left: 1px solid #bfdcf0;
    border-top: 4px solid #0072c5;
    background-color: #f2f8fc;
}

#rcTB {
    /*float: right;*/
    float: left;
    padding-top: 5px;
    max-width: 350px;
}

.wordwrap {
    /*word-wrap: break-word;*/
    word-break: break-all;
}

#rcCalB input {
    height: 50px;
    width: 84px;
    cursor: pointer;
    margin: 3px;
}

.calc_variable {
    overflow:hidden;
    white-space:nowrap;
    text-overflow: ellipsis;/* IE/Safari */
    -ms-text-overflow: ellipsis;
    -o-text-overflow: ellipsis;/* Opera */
    -moz-binding: url("ellipsis.xml#ellipsis");/*FireFox*/
}

.rcOptB {
    background-color: #eee;
    color: #666;
    border: 1px solid #ddd;
}

.rcOptB:hover {
    background-color: #bbb;
    border: 1px solid #bbb;
}

.rcOptB:active {
    background-color: #ccc;
    border: 1px solid #ccc;
}

.rcOpdB {
    background-color: #d1d1d1;
    color: #111;
    border: 1px solid #c1c1c1;
}

.rcOpdB:hover {
    background-color: #bbb;
    border: 1px solid #bbb;
}

.rcOpdB:active {
    background-color: #ccc;
    border: 1px solid #ccc;
}

.rcRB {
    background-color: #0072c5;
    color: #fff;
    border: 1px solid #0072c5;
}

.rcRB:hover {
    background-color: #005b9e;
    border: 1px solid #005b9e;
}

.rcRB:active {
    background-color: #004476;
    border: 1px solid #004476;
}

#rcBB {
    background-image: url('../img/back.png');
    background-position: center;
    background-repeat: no-repeat;
}

.b_xlText {
    font-size: 24px;
    line-height: 30px;
}

.b_focusTextExtraSmall, .b_xlText {
    font: 24px/1.2em 'Microsoft YaHei',Arial,Helvetica,sans-serif;
}

table {
    /*width: 100%;*/
    word-wrap: break-word;
}

.calc_div {
    margin-top: 40px;
    margin-bottom: 40px;
    display: inline-flex;
    width: 100%;
    height: 565px;
}

#calc_main {
    margin-bottom: 40px;
}

.calc-form-inline {
    margin-left: 20px;
}

.calc-form-inline .form-group {
    display: inline-block;
    vertical-align: middle;
}

#calc_search_btn {
    margin-top: 24px;
    margin-left: 15px;
}

.zero-delete {
    display: block;
    position: relative;
}

.equation-delete {
    position: absolute;
    right: -3px;
    z-index: 10;
    display: block;
    padding: 0 6px 1px 6px;
    font-size: 12px;
    color: #777;
    cursor: pointer;
    background-color: #fff;
    border: 1px solid #777;
    border-radius: 10px;
}

.equation-delete:hover {
    color: #fff;
    background-color: #DC3410;
    border-color: #DC3410;

}

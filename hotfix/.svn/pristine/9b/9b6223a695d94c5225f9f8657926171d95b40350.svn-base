<script type="text/javascript" src="/2016/lib/jquery/fileupload.min.js"></script>
<script type="text/javascript" src="/2016/lib/jquery/function.js"></script>
<script type="text/javascript" src="/2016/src/common.js?v={{ time_stamp }}"></script>
<script type="text/javascript" src="/2016/src/captcha.js?v={{ time_stamp }}"></script>
<script type="text/javascript" src="/2016/src/personalCenterSbasic.js?v={{ time_stamp }}"></script>
{% endblock %}
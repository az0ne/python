
.scholarship-box{
	width:1200px;
	margin: 0 auto;
	overflow: hidden;
	padding: 20px;
	color: #686868;
}



/************************得分概况*************************/

.score-overview-content{
	width: 100%;
	box-sizing: border-box;
	background-color: #fff;
	padding: 20px;
	margin-bottom: 20px;
}
.score-overview-title{
	font-size: 18px;
	margin-bottom: 20px;
	color: #FF6500;
}
.score-overview-title-li{
	font-size: 16px;
	font-weight: bold;
}
.score-overview-progres{
	width: 100%;
}
.progress-bar-div{
	width: 548px;
	height: 158px;
	box-sizing: border-box;
	padding: 20px 30px;
	border: 1px solid #DADADA ;
	float: left;

}

